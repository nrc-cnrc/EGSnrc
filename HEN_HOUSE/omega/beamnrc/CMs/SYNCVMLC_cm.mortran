   (//' ***ERROR IN CM ',I3,' ($SYNCVMLC) in FULL leaf definition:'/
      ' WRAILTOP_$SYNCVMLC(1)>LEAFWIDTH_$SYNCVMLC(1)+WGROOVE_$SYNCVMLC(1).'/
      ' Upper rail too wide.'//);
        IERR_GEOM(ICM_$SYNCVMLC)=IERR_GEOM(ICM_$SYNCVMLC)+1;
]
IF(WRAILTOP_$SYNCVMLC(1)<WTIP_$SYNCVMLC(1)-WTONGUE_$SYNCVMLC(1))[

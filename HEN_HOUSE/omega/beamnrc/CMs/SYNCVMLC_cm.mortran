         IERR_GEOM(ICM_$SYNCVMLC)=IERR_GEOM(ICM_$SYNCVMLC)+1;
]
IF(WTIP_$SYNCVMLC(1)>LEAFWIDTH_$SYNCVMLC(1)+WTONGUE_$SYNCVMLC(1))[

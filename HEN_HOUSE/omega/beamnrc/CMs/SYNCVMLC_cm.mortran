END;

%E "Start of subroutine HOWNEAR_$SYNCVMLC (Rev 1.4)"
"***********************************************************************"
"                                                                       "
"                          Subroutine HOWNEAR_$SYNCVMLC               "
"                          ******************************               "
"                                                                       "
" Calculates min. distance to nearest region boundary                   "
" Used to be HOWNEAR macro, but is now called from that macro.          "
"                                                                       "
"***********************************************************************"

;SUBROUTINE HOWNEAR_$SYNCVMLC(DIST_sngl);
;
$IMPLICIT-NONE;

COMIN/CMs,CM_$SYNCVMLC,STACK,USER,EGS-IO/;

$REAL   DIST_sngl; "T> min. distance to nearest region boundary"
DOUBLE PRECISION   XYL1,XYL2, "T> X(NP) and Y(NP)"
                   XoN, XoP, DIST,
                   UVL1,UVL2,
                   TEMP1,TEMP2,TEMP3,TEMP4,
                   TEMP5,TEMP6,HOLE, "T> temp. distance variables"
                   XL,XR,Lo,Ro,Zo;          "T>temp distance variables"
$INTEGER I,J, "T> looping index"
         NZ, "T> index of subregion in Z direction"
         NY, "T> index of subregion perpendicular to leaf opening direction"
         NX, "T> index of subregion in direction of leaf opening"
         LEAFIS, "T> leaf no. where particle is located"
         I1,I2; "T> used to mark min. max. Z boundaries for calculating dist"


IR_$SYNCVMLC=IR(NP)-IRSTART_$SYNCVMLC+1;
IF(IR_$SYNCVMLC=4) [ "in the air gap at the top"
  DIST=MIN(Z(NP)-ZFRONT_$SYNCVMLC,ZMIN_$SYNCVMLC-Z(NP));
  ]

ELSE [
  IF(ORIENT_$SYNCVMLC=1) [ XYL1=Y(NP);XYL2=X(NP);UVL1=V(NP);UVL2=U(NP); ]
  ELSE [ XYL1=X(NP);XYL2=Y(NP);UVL1=U(NP);UVL2=V(NP);];

  IF(IGNOREGAPS_$SYNCVMLC=1 & XYL2 < MIN_PLANE_$SYNCVMLC &
  XYL1 > SURPARA1_$SYNCVMLC(1,1)*(Z(NP)-ZFOCUS_$SYNCVMLC(1)) &
  XYL1 < SURPARA1_$SYNCVMLC(TOT_LEAF_$SYNCVMLC,7)*(Z(NP)-ZFOCUS_$SYNCVMLC(1)) &
  IR_$SYNCVMLC=2)[
   "particle within negative leaves, ignore air gaps for range rejection"

   "distance to most -ve leaf side...note this is to the tip of the tongue"
   TEMP1=SURPARA1_$SYNCVMLC(1,1);
   TEMP1=ABS((TEMP1*(Z(NP)-ZFOCUS_$SYNCVMLC(1))-XYL1)/SQRT(1+TEMP1**2));

   "distance to most +ve leaf side...note this is to the back of the last"
   "leaf...ie not to the groove"
   TEMP2=SURPARA1_$SYNCVMLC(TOT_LEAF_$SYNCVMLC,7);
   TEMP2=ABS((TEMP2*(Z(NP)-ZFOCUS_$SYNCVMLC(1))-XYL1)/SQRT(1+TEMP2**2));

   DIST=MIN(Z(NP)-ZMIN_$SYNCVMLC,ZMIN_$SYNCVMLC+ZTHICK_$SYNCVMLC-Z(NP),
           MIN_PLANE_$SYNCVMLC-XYL2,TEMP1,TEMP2);
  ]
  ELSEIF(IGNOREGAPS_$SYNCVMLC=1 & XYL2 > MAX_PLANE_$SYNCVMLC &
  XYL1 > SURPARA1_$SYNCVMLC(1,1)*(Z(NP)-ZFOCUS_$SYNCVMLC(1)) &
  XYL1 < SURPARA1_$SYNCVMLC(TOT_LEAF_$SYNCVMLC,7)*(Z(NP)-ZFOCUS_$SYNCVMLC(1)) &
  IR_$SYNCVMLC=2)[
   "particle within positive leaves, ignore air gaps for range rejection"

   "distance to most -ve leaf side...note this is to the tip of the tongue"
   TEMP1=SURPARA1_$SYNCVMLC(1,1);
   TEMP1=ABS((TEMP1*(Z(NP)-ZFOCUS_$SYNCVMLC(1))-XYL1)/SQRT(1+TEMP1**2));

   "distance to most +ve leaf side...note this is to the back of the last"
   "leaf...ie not just to the groove"
   TEMP2=SURPARA1_$SYNCVMLC(TOT_LEAF_$SYNCVMLC,7);
   TEMP2=ABS((TEMP2*(Z(NP)-ZFOCUS_$SYNCVMLC(1))-XYL1)/SQRT(1+TEMP2**2));

   DIST=MIN(Z(NP)-ZMIN_$SYNCVMLC,ZMIN_$SYNCVMLC+ZTHICK_$SYNCVMLC-Z(NP),
           XYL2-MAX_PLANE_$SYNCVMLC,TEMP1,TEMP2);
  ]
  ELSE["do not ignore air gaps for range rejection"

LEAFIS=0; "Determine which leaf we are in, I is index of leaf number"
DO I = 1,TOT_LEAF_$SYNCVMLC [
    TEMP1=SURPARA1_$SYNCVMLC(I,1)*(Z(NP)-ZFOCUS_$SYNCVMLC(1));
    TEMP2=SURPARA1_$SYNCVMLC(I,7)*(Z(NP)-ZFOCUS_$SYNCVMLC(1));
    IF(XYL1>=TEMP1 & TEMP2>=XYL1) [
       LEAFIS=I; EXIT;];
 ];

IF(LEAFIS~=0) [
  TEMP1=SURPARA1_$SYNCVMLC(LEAFIS,1)*(Z(NP)-ZFOCUS_$SYNCVMLC(1));
  TEMP2=SURPARA1_$SYNCVMLC(LEAFIS,7)*(Z(NP)-ZFOCUS_$SYNCVMLC(1));
  IF(XYL1<TEMP1)[
    NY=1;
  ]
  ELSEIF(XYL1>TEMP2)[
    NY=6;
  ]
  ELSE[

    DO J =1,6 [ "Determine which Y region we are in"
      TEMP1=SURPARA1_$SYNCVMLC(LEAFIS,J)*(Z(NP)-ZFOCUS_$SYNCVMLC(1));
      TEMP2=SURPARA1_$SYNCVMLC(LEAFIS,J+1)*(Z(NP)-ZFOCUS_$SYNCVMLC(1));
      IF(TEMP1<=XYL1 & XYL1<=TEMP2) [NY=J; EXIT;]
      ];
  ]
  IF(Z(NP)<ZREG_$SYNCVMLC(LEAFIS,1))[
    NZ=1;
  ]
  ELSEIF(Z(NP)>ZREG_$SYNCVMLC(LEAFIS,10))[
    NZ=9;
  ]
  ELSE[
    DO J = 1,9 [ "Determine which Z region we are in for leaf I"
   IF((ZREG_$SYNCVMLC(LEAFIS,J)<=Z(NP))&(Z(NP)<=ZREG_$SYNCVMLC(LEAFIS,J+1)))[
       NZ=J; EXIT;];
    ];
  ]

  "Check if we are in groove on leaf and move to adjacent leaf"
  IF(LEAFTYPE_$SYNCVMLC(LEAFIS)=2) [ "target leaf"
    IF((NY=5|NY=6) & (NZ=7|NZ=8|NZ=9) & LEAFIS~=TOT_LEAF_$SYNCVMLC) [
       IF(XYL1>SURPARA1_$SYNCVMLC(LEAFIS+1,1)*(Z(NP)-ZFOCUS_$SYNCVMLC(1)))[
          LEAFIS=LEAFIS+1;
          TEMP1=SURPARA1_$SYNCVMLC(LEAFIS,1)*(Z(NP)-ZFOCUS_$SYNCVMLC(1));
          TEMP2=SURPARA1_$SYNCVMLC(LEAFIS,7)*(Z(NP)-ZFOCUS_$SYNCVMLC(1));
          IF(XYL1<TEMP1)[
            NY=1;
          ]
          ELSEIF(XYL1>TEMP2)[
            NY=6;
          ]
          ELSE[
            DO J=1,6 [ "recheck Y region"
             TEMP1=SURPARA1_$SYNCVMLC(LEAFIS,J)*(Z(NP)-ZFOCUS_$SYNCVMLC(1));
             TEMP2=SURPARA1_$SYNCVMLC(LEAFIS,J+1)*(Z(NP)-ZFOCUS_$SYNCVMLC(1));
             IF(TEMP1<=XYL1 & XYL1<=TEMP2) [NY=J; EXIT;]
            ];
          ]
          IF(Z(NP)<ZREG_$SYNCVMLC(LEAFIS,1))[
            NZ=1;
          ]
          ELSEIF(Z(NP)>ZREG_$SYNCVMLC(LEAFIS,10))[
            NZ=9;
          ]
          ELSE[
            DO J= 1,9 [ "recheck Z region"
              IF((ZREG_$SYNCVMLC(LEAFIS,J)<=Z(NP))
               & (Z(NP)<=ZREG_$SYNCVMLC(LEAFIS,J+1)) ) [NZ=J; EXIT;];
            ];
          ]
         ];
       ];
      ]
    ELSE [ "we are on a full leaf or isocenter leaf"
       IF(NY=6 & NZ<=4 & LEAFIS~=TOT_LEAF_$SYNCVMLC) [
          IF(XYL1>SURPARA1_$SYNCVMLC(LEAFIS+1,1)*(Z(NP)-ZFOCUS_$SYNCVMLC(1)))[
             LEAFIS=LEAFIS+1; NY=1;
             IF(Z(NP)<ZREG_$SYNCVMLC(LEAFIS,1))[
                NZ=1;
             ]
             ELSEIF(Z(NP)>ZREG_$SYNCVMLC(LEAFIS,10))[
                NZ=9;
             ]
             ELSE[
               DO J=1,9 [ "recheck Z region"
                  IF((ZREG_$SYNCVMLC(LEAFIS,J)<=Z(NP))
                   & (Z(NP)<=ZREG_$SYNCVMLC(LEAFIS,J+1))) [NZ=J; EXIT;];
               ];
             ]
             "***check that we have not moved into air gap on next leaf**"
             IF(LEAFTYPE_$SYNCVMLC(LEAFIS)=2) [
                "target leaf"
                IF(NZ>=6) [LEAFIS=LEAFIS-1; NY=6; "****************"
                  IF(Z(NP)<ZREG_$SYNCVMLC(LEAFIS,1))[
                    NZ=1;
                  ]
                  ELSEIF(Z(NP)>ZREG_$SYNCVMLC(LEAFIS,10))[
                    NZ=9;
                  ]
                  ELSE[
                       DO J=1,9 [ "recheck Z"
                         IF((ZREG_$SYNCVMLC(LEAFIS,J)<=Z(NP))
                         & (Z(NP)<=ZREG_$SYNCVMLC(LEAFIS,J+1))) [NZ=J; EXIT;];
                       ];
                  ]
                ];
             ]
             ELSE ["full leaf"
                 IF(NZ>=4) [LEAFIS=LEAFIS-1; NY=6; "****************"
                  IF(Z(NP)<ZREG_$SYNCVMLC(LEAFIS,1))[
                    NZ=1;
                  ]
                  ELSEIF(Z(NP)>ZREG_$SYNCVMLC(LEAFIS,10))[
                    NZ=9;
                  ]
                  ELSE[
                   DO J=1,9 [ "recheck Z"
                    IF((ZREG_$SYNCVMLC(LEAFIS,J)<=Z(NP))
                    & (Z(NP)<=ZREG_$SYNCVMLC(LEAFIS,J+1))) [NZ=J; EXIT;];
                   ];
                  ]
                 ];
                ]
           ];
         ];
      ];

  "Determine which X region we are in"
  IF(ENDTYPE_$SYNCVMLC=1) [ "Focused leaf end"
    TEMP1=SURPARA2_B_$SYNCVMLC(LEAFIS)*(Z(NP)-ZFOCUS_$SYNCVMLC(2));
    TEMP2=SURPARA2_A_$SYNCVMLC(LEAFIS)*(Z(NP)-ZFOCUS_$SYNCVMLC(2));
    HOLE=HOLEPOS_$SYNCVMLC(LEAFIS);
    TEMP3=LEAFB_$SYNCVMLC(LEAFIS)-HOLE;
    TEMP4=LEAFA_$SYNCVMLC(LEAFIS)+HOLE;
    IF((XYL2>TEMP1 & XYL2<TEMP2) | (XYL2=TEMP1 & UVL2>=0.0) |
       (XYL2=TEMP2 & UVL2<0.0)) [NX=1;]
    ELSEIF((XYL2<TEMP1 & XYL2>TEMP3) | (XYL2=TEMP1 & UVL2>=0.0) |
           (XYL2=TEMP3 & UVL2>=0.0)) [NX=3;]
    ELSEIF((XYL2<TEMP3) | (XYL2=TEMP3 & UVL2<0.0)) [NX=2;]
    ELSEIF((XYL2>TEMP2 & XYL2<TEMP4) | (XYL2=TEMP2 & UVL2>=0.0) |
           (XYL2=TEMP4 & UVL2<0.0)) [NX=4;]
    ELSEIF((XYL2>TEMP4) | (XYL2=TEMP4 & UVL2>=0.0)) [NX=5;]
  ]
  ELSE ["Rounded leaf end"
    XoN=LEAFB_$SYNCVMLC(LEAFIS)-LEAFRADIUS_$SYNCVMLC;
    XoP=LEAFA_$SYNCVMLC(LEAFIS)+LEAFRADIUS_$SYNCVMLC;
    Zo=ZMIN_$SYNCVMLC + (ZTHICK_$SYNCVMLC)/2;
    TEMP1=SQRT((XYL2-XoN)**2+(Zo-Z(NP))**2)-LEAFRADIUS_$SYNCVMLC;
    TEMP2=SQRT((XYL2-XoP)**2+(Zo-Z(NP))**2)-LEAFRADIUS_$SYNCVMLC;
    HOLE=HOLEPOS_$SYNCVMLC(LEAFIS);
    TEMP3=LEAFB_$SYNCVMLC(LEAFIS)-HOLE;
    TEMP4=LEAFA_$SYNCVMLC(LEAFIS)+HOLE;
    IF(XYL2>=XoN & XYL2<=XoP & ((TEMP1>0.0 & TEMP2>0.0) |
       (TEMP1=0.0 & UVL2*(XYL2-XoN)+W(NP)*(Z(NP)-Zo)>0.0) |
       (TEMP2=0.0 & UVL2*(XYL2-XoP)+W(NP)*(Z(NP)-Zo)>0.0))) [NX=1;]
    ELSEIF((TEMP1<0.0 & XYL2>TEMP3) | (XYL2=TEMP3 & UVL2>0.0) |
           (TEMP1=0.0 & UVL2*(XYL2-XoN)+W(NP)*(Z(NP)-Zo)<=0.0)) [NX=3;]
    ELSEIF((XYL2<TEMP3) | (XYL2=TEMP3 & UVL2<=0.0)) [NX=2;]
    ELSEIF((TEMP2<0.0 & XYL2<TEMP4) | (XYL2=TEMP4 & UVL2<0.0) |
           (TEMP2=0.0 & UVL2*(XYL2-XoP)+W(NP)*(Z(NP)-Zo)<=0.0)) [NX=4;]
    ELSEIF((XYL2>TEMP4) | (XYL2=TEMP4 & UVL2>=0.0)) [NX=5;]
  ];
   "Set minimum and maximum Z boundaries"
    IF(LEAFTYPE_$SYNCVMLC(LEAFIS)=2) ["Target leaf"
      IF((NY=1|NY=2) & NZ<=3) [I1=1; I2=4;]
      ELSEIF(NY=1 & NZ=5) [I1=5; I2=6;]
      ELSEIF(NY=1 & NZ>=6) [I1=6; I2=10;]
      ELSEIF(NZ=4) [I1=4; I2=5;]
      ELSEIF((NY=2|NY=3) & NZ>=8) [I1=8; I2=10;]
      ELSEIF((NY=3|NY=4|NY=5) & NZ=1) [I1=1; I2=2;]
      ELSEIF((NY=3|NY=4|NY=5) & NZ=2) [I1=2; I2=3;]
      ELSEIF(NY>=3 & NZ=3) [I1=3; I2=4;]
      ELSEIF((NY=2|NY=3) & (NZ=5|NZ=6|NZ=7)) [I1=5; I2=8;]
      ELSEIF(NY=4 & (NZ=5|NZ=6|NZ=7|NZ=8)) [I1=5; I2=9;]
      ELSEIF(NY=4 & NZ=9) [I1=9; I2=10;]
      ELSEIF((NY=5|NY=6) & (NZ=5|NZ=6)) [I1=5; I2=7;]
      ELSEIF((NY=5|NY=6) & NZ>=7) [I1=7; I2=10;]
      ELSEIF(NY=6 & NZ<=2) [I1=1; I2=3;]
     ]
    ELSEIF(LEAFTYPE_$SYNCVMLC(LEAFIS)=1) ["Full leaf"
      IF((NY=1|NY=2) & NZ=1) [I1=1; I2=2;]
      ELSEIF(NY=1 & (NZ=2|NZ=3)) [I1=2; I2=4;]
      ELSEIF(NY=1 & NZ>=4) [I1=4; I2=10;]
      ELSEIF(NY=2 & (NZ=2|NZ=3|NZ=4|NZ=5)) [I1=2; I2=6;]
      ELSEIF(NY>=2 & NZ=6) [I1=6; I2=7;]
      ELSEIF((NY=2|NY=3|NY=4) & NZ=7) [I1=7; I2=8;]
      ELSEIF(NY=2 & NZ>=8) [I1=8; I2=10;]
      ELSEIF((NY=3|NY=4|NY=5) & (NZ=1|NZ=2)) [I1=1; I2=3;]
      ELSEIF((NY=3|NY=4|NY=5) & (NZ=3|NZ=4|NZ=5)) [I1=3; I2=6;]
      ELSEIF(NY=3 & NZ>=8) [I1=8; I2=10;]
      ELSEIF(NY=4 & NZ=8) [I1=8; I2=9;]
      ELSEIF(NY=4 & NZ=9) [I1=9; I2=10;]
      ELSEIF((NY=5|NY=6) & NZ>=7) [I1=7; I2=10;]
      ELSEIF(NY=6 & NZ<=4) [I1=1; I2=5;]
      ELSEIF(NY=6 & NZ=5) [I1=5; I2=6;]
     ]
    ELSE ["Isocenter leaf"
      IF((NY=1|NY=2) & NZ<=3) [I1=1; I2=4;]
      ELSEIF((NY=1|NY=2) & (NZ=4|NZ=5)) [I1=4; I2=6;]
      ELSEIF(NZ=6) [I1=6; I2=7;]
      ELSEIF(NY<=4 & NZ=7) [I1=7; I2=8;]
      ELSEIF(NY=1 & NZ>=8) [I1=8; I2=10;]
      ELSEIF((NY=2|NY=3|NY=4) & NZ=8) [I1=8; I2=9;]
      ELSEIF((NY=2|NY=3|NY=4) & NZ=9) [I1=9; I2=10;]
      ELSEIF(NY=3 & NZ=1) [I1=1; I2=2;]
      ELSEIF(NY=3 & (NZ=2|NZ=3|NZ=4|NZ=5)) [I1=2; I2=6;]
      ELSEIF((NY=4|NY=5) & NZ<=2) [I1=1; I2=3;]
      ELSEIF((NY=4|NY=5) & (NZ=3|NZ=4|NZ=5)) [I1=3; I2=6;]
      ELSEIF((NY=5|NY=6) & NZ>=7) [I1=7; I2=10;]
      ELSEIF(NY=6 & NZ<=4) [I1=1; I2=5;]
      ELSEIF(NY=6 & NZ=5) [I1=5; I2=6;]
     ];

    DIST=MIN(ABS(ZREG_$SYNCVMLC(LEAFIS,I1)-Z(NP)),
             ABS(ZREG_$SYNCVMLC(LEAFIS,I2)-Z(NP)));

    "Calculates distance to closest boundary in z direction"
    "Calculate distance to nearest boundary perp to leaf openings --->"
    IF(LEAFTYPE_$SYNCVMLC(LEAFIS)=2) ["Target leaf"
      IF(NZ=1 & NY<=5) [
        TEMP1=SURPARA1_$SYNCVMLC(LEAFIS,1);
        TEMP2=SURPARA1_$SYNCVMLC(LEAFIS,6);
       ]
     ELSEIF(NZ=1 & NY=6) [
        TEMP1=SURPARA1_$SYNCVMLC(LEAFIS,6);
        IF(LEAFIS=TOT_LEAF_$SYNCVMLC) [
          TEMP2=SURPARA1_$SYNCVMLC(LEAFIS,7);]
        ELSE [TEMP2=SURPARA1_$SYNCVMLC(LEAFIS+1,3);]
       ]
     ELSEIF(NZ=2 & NY<=2) [
        TEMP1=SURPARA1_$SYNCVMLC(LEAFIS,1);
        TEMP2=SURPARA1_$SYNCVMLC(LEAFIS,3);
       ]
     ELSEIF(NZ=2 & NY>=3) [
        TEMP1=SURPARA1_$SYNCVMLC(LEAFIS,3);
        IF(LEAFIS=TOT_LEAF_$SYNCVMLC) [
          TEMP2=SURPARA1_$SYNCVMLC(LEAFIS,7);]
        ELSE [TEMP2=SURPARA1_$SYNCVMLC(LEAFIS+1,3);]
       ]
     ELSEIF(NZ=3|NZ=4|NZ=5) [
        TEMP1=SURPARA1_$SYNCVMLC(LEAFIS,1);
        TEMP2=SURPARA1_$SYNCVMLC(LEAFIS,7);
       ]
     ELSEIF(NY=1 & (NZ=6|NZ=7)) [
        IF(LEAFIS=1) [TEMP1=SURPARA1_$SYNCVMLC(LEAFIS,1);]
        ELSE [TEMP1=SURPARA1_$SYNCVMLC(LEAFIS-1,7);]
        TEMP2=SURPARA1_$SYNCVMLC(LEAFIS,2);
       ]
     ELSEIF(NY>=2 & NZ=6) [
       TEMP1=SURPARA1_$SYNCVMLC(LEAFIS,2);
       TEMP2=SURPARA1_$SYNCVMLC(LEAFIS,7);
       ]
     ELSEIF(NZ=7 & (NY=2|NY=3|NY=4)) [
       TEMP1=SURPARA1_$SYNCVMLC(LEAFIS,2);
       TEMP2=SURPARA1_$SYNCVMLC(LEAFIS,5);
       ]
     ELSEIF((NZ=7|NZ=8) & NY>=5) [
       TEMP1=SURPARA1_$SYNCVMLC(LEAFIS,5);
       IF(LEAFIS=TOT_LEAF_$SYNCVMLC) [
         TEMP2=SURPARA1_$SYNCVMLC(LEAFIS,7);]
       ELSE [TEMP2=SURPARA1_$SYNCVMLC(LEAFIS+1,1);]
      ]
     ELSEIF(NZ=8 & NY<=3) [IF(LEAFIS=1) [
       TEMP1=SURPARA1_$SYNCVMLC(LEAFIS,1);]
       ELSE [TEMP1=SURPARA1_$SYNCVMLC(LEAFIS-1,7);]
       TEMP2=SURPARA1_$SYNCVMLC(LEAFIS,4);
      ]
     ELSEIF(NZ=8 & NY=4) [
       TEMP1=SURPARA1_$SYNCVMLC(LEAFIS,4);
       TEMP2=SURPARA1_$SYNCVMLC(LEAFIS,5);
      ]
     ELSEIF(NZ=9) [IF(LEAFIS=1) [
       TEMP1=SURPARA1_$SYNCVMLC(LEAFIS,1);]
       ELSE [TEMP1=SURPARA1_$SYNCVMLC(LEAFIS-1,7);]
       IF(LEAFIS=TOT_LEAF_$SYNCVMLC) [
         TEMP2=SURPARA1_$SYNCVMLC(LEAFIS,7);]
       ELSE [TEMP2=SURPARA1_$SYNCVMLC(LEAFIS+1,1);]
     ]
    ]
   ELSEIF(LEAFTYPE_$SYNCVMLC(LEAFIS)=1) ["Full leaf"
      IF(NZ=1) [
        IF(LEAFIS=1) [TEMP1=SURPARA1_$SYNCVMLC(LEAFIS,1);]
        ELSE [TEMP1=SURPARA1_$SYNCVMLC(LEAFIS-1,6);]
        IF(LEAFIS=TOT_LEAF_$SYNCVMLC) [TEMP2=SURPARA1_$SYNCVMLC(LEAFIS,7);]
        ELSE [TEMP2=SURPARA1_$SYNCVMLC(LEAFIS+1,1);]
       ]
      ELSEIF(NZ=2 & NY<=2) [
        TEMP1=SURPARA1_$SYNCVMLC(LEAFIS,1);
        TEMP2=SURPARA1_$SYNCVMLC(LEAFIS,3);
       ]
      ELSEIF(NZ=2 & NY>=3) [
        TEMP1=SURPARA1_$SYNCVMLC(LEAFIS,3);
        IF(LEAFIS=TOT_LEAF_$SYNCVMLC) [TEMP2=SURPARA1_$SYNCVMLC(LEAFIS,7);]
        ELSE [TEMP2=SURPARA1_$SYNCVMLC(LEAFIS+1,1);]
       ]
      ELSEIF(NZ=3 & NY<=5) [
        TEMP1=SURPARA1_$SYNCVMLC(LEAFIS,1);
        TEMP2=SURPARA1_$SYNCVMLC(LEAFIS,6);
       ]
      ELSEIF((NZ=3|NZ=4) & NY=6) [
        TEMP1=SURPARA1_$SYNCVMLC(LEAFIS,6);
        IF(LEAFIS=TOT_LEAF_$SYNCVMLC) [TEMP2=SURPARA1_$SYNCVMLC(LEAFIS,7);]
        ELSE [TEMP2=SURPARA1_$SYNCVMLC(LEAFIS+1,1);]
       ]
      ELSEIF((NZ=4|NZ=5|NZ=6|NZ=7) & NY=1) [
        IF(LEAFIS=1) [TEMP1=SURPARA1_$SYNCVMLC(LEAFIS,1);]
        ELSE [TEMP1=SURPARA1_$SYNCVMLC(LEAFIS-1,7);]
        TEMP2=SURPARA1_$SYNCVMLC(LEAFIS,2);
       ]
      ELSEIF(NZ=4 & (NY=2|NY=3|NY=4|NY=5)) [
        TEMP1=SURPARA1_$SYNCVMLC(LEAFIS,2);
        TEMP2=SURPARA1_$SYNCVMLC(LEAFIS,6);
       ]
      ELSEIF((NZ=5|NZ=6|NZ=7) & NY>=2) [
        TEMP1=SURPARA1_$SYNCVMLC(LEAFIS,2);
        TEMP2=SURPARA1_$SYNCVMLC(LEAFIS,7);
       ]
      ELSEIF(NZ=8 & NY<=4) [
        IF(LEAFIS=1) [TEMP1=SURPARA1_$SYNCVMLC(LEAFIS,1);]
        ELSE [TEMP1=SURPARA1_$SYNCVMLC(LEAFIS-1,7);]
        TEMP2=SURPARA1_$SYNCVMLC(LEAFIS,5);
       ]
      ELSEIF(NZ=8 & NY>=5) [
        TEMP1=SURPARA1_$SYNCVMLC(LEAFIS,5);
        TEMP2=SURPARA1_$SYNCVMLC(LEAFIS,7);
       ]
      ELSEIF(NZ=9 & NY<=3) [
        IF(LEAFIS=1) [TEMP1=SURPARA1_$SYNCVMLC(LEAFIS,1);]
        ELSE [TEMP1=SURPARA1_$SYNCVMLC(LEAFIS-1,7);]
        TEMP2=SURPARA1_$SYNCVMLC(LEAFIS,4);
       ]
      ELSEIF(NZ=9 & NY>=4) [
        TEMP1=SURPARA1_$SYNCVMLC(LEAFIS,4);
        TEMP2=SURPARA1_$SYNCVMLC(LEAFIS,7);
       ]
     ]
    ELSE ["Isocenter leaf"
      IF(NZ=1) [
        IF(LEAFIS=1) [TEMP1=SURPARA1_$SYNCVMLC(LEAFIS,1);]
        ELSE [TEMP1=SURPARA1_$SYNCVMLC(LEAFIS-1,7);];
        IF(LEAFIS=TOT_LEAF_$SYNCVMLC) [TEMP2=SURPARA1_$SYNCVMLC(LEAFIS,7);]
        ELSE [TEMP2=SURPARA1_$SYNCVMLC(LEAFIS+1,1);];
       ]
      ELSEIF((NZ=2|NZ=3) & (NY=1|NY=2)) [
        IF(LEAFIS=1) [TEMP1=SURPARA1_$SYNCVMLC(LEAFIS,1);]
        ELSE [TEMP1=SURPARA1_$SYNCVMLC(LEAFIS-1,7);];
        TEMP2=SURPARA1_$SYNCVMLC(LEAFIS,3);
       ]
      ELSEIF(NZ=2 & NY=3) [
        TEMP1=SURPARA1_$SYNCVMLC(LEAFIS,3);
        TEMP2=SURPARA1_$SYNCVMLC(LEAFIS,4);
       ]
      ELSEIF(NZ=2 & NY>=4) [
        TEMP1=SURPARA1_$SYNCVMLC(LEAFIS,4);
        IF(LEAFIS=TOT_LEAF_$SYNCVMLC) [TEMP2=SURPARA1_$SYNCVMLC(LEAFIS,7);]
        ELSE [TEMP2=SURPARA1_$SYNCVMLC(LEAFIS+1,1);];
       ]
      ELSEIF(NZ=3 & (NY=3|NY=4|NY=5)) [
        TEMP1=SURPARA1_$SYNCVMLC(LEAFIS,3);
        TEMP2=SURPARA1_$SYNCVMLC(LEAFIS,6);
       ]
      ELSEIF((NZ=3|NZ=4) & NY=6) [
        TEMP1=SURPARA1_$SYNCVMLC(LEAFIS,6);
        IF(LEAFIS=TOT_LEAF_$SYNCVMLC) [TEMP2=SURPARA1_$SYNCVMLC(LEAFIS,7);]
        ELSE [TEMP2=SURPARA1_$SYNCVMLC(LEAFIS+1,1);];
       ]
      ELSEIF(NZ=4 & NY<=5) [
        TEMP1=SURPARA1_$SYNCVMLC(LEAFIS,1);
        TEMP2=SURPARA1_$SYNCVMLC(LEAFIS,6);
       ]
      ELSEIF(NZ=5|NZ=6|NZ=7) [
        TEMP1=SURPARA1_$SYNCVMLC(LEAFIS,1);
        TEMP2=SURPARA1_$SYNCVMLC(LEAFIS,7);
       ]
      ELSEIF(NZ=8 & NY<=4) [
        IF(LEAFIS=1) [TEMP1=SURPARA1_$SYNCVMLC(LEAFIS,1);]
        ELSE [TEMP1=SURPARA1_$SYNCVMLC(LEAFIS-1,5);];
        TEMP2=SURPARA1_$SYNCVMLC(LEAFIS,5);
       ]
      ELSEIF(NZ=8 & NY>=5) [
        TEMP1=SURPARA1_$SYNCVMLC(LEAFIS,5);
        TEMP2=SURPARA1_$SYNCVMLC(LEAFIS,7);
       ]
      ELSEIF(NZ=9 & NY=1) [
        IF(LEAFIS=1) [TEMP1=SURPARA1_$SYNCVMLC(LEAFIS,1);]
        ELSE [TEMP1=SURPARA1_$SYNCVMLC(LEAFIS-1,5);]
        TEMP2=SURPARA1_$SYNCVMLC(LEAFIS,2);
       ]
      ELSEIF(NZ=9 & NY>=2) [
        TEMP1=SURPARA1_$SYNCVMLC(LEAFIS,2);
        TEMP2=SURPARA1_$SYNCVMLC(LEAFIS,7);
       ]
     ];
    TEMP1=ABS((TEMP1*(Z(NP)-ZFOCUS_$SYNCVMLC(1))-XYL1)/
          SQRT(1+TEMP1**2));
    TEMP2=ABS((TEMP2*(Z(NP)-ZFOCUS_$SYNCVMLC(1))-XYL1)/
          SQRT(1+TEMP2**2));

    DIST = MIN(DIST,TEMP1,TEMP2);
 "NOW CHECK DISTANCE IN X DIRECTION"
    IF(ENDTYPE_$SYNCVMLC=1) [ "focused leaf end"
      TEMP1=SURPARA2_B_$SYNCVMLC(LEAFIS);
      TEMP1=1/SQRT(1+TEMP1**2);
      TEMP2=SURPARA2_A_$SYNCVMLC(LEAFIS);
      TEMP2=1/SQRT(1+TEMP2**2);
      HOLE=HOLEPOS_$SYNCVMLC(LEAFIS);
      TEMP3=LEAFB_$SYNCVMLC(LEAFIS)-HOLE;
      TEMP4=LEAFA_$SYNCVMLC(LEAFIS)+HOLE;
      TEMP5=SURPARA2_B_$SYNCVMLC(LEAFIS)*(Z(NP)-ZFOCUS_$SYNCVMLC(2));
      TEMP6=SURPARA2_A_$SYNCVMLC(LEAFIS)*(Z(NP)-ZFOCUS_$SYNCVMLC(2));
      IF(NX=1) [
        XL=ABS((TEMP5-XYL2)*TEMP1);
        XR=ABS((TEMP6-XYL2)*TEMP2);
       ]
      ELSEIF(NX=2) [
        XL=ABS(RMAX_CM(ICM_$SYNCVMLC)-XYL2);
        XR=ABS(TEMP3-XYL2);
       ]
      ELSEIF(NX=3) [
        XL=ABS(XYL2-TEMP3);
        XR=ABS((TEMP5-XYL2)*TEMP1);
       ]
      ELSEIF(NX=4) [
        XL=ABS((XYL2-TEMP6)*TEMP2);
        XR=ABS(XYL2-TEMP4);
       ]
      ELSEIF(NX=5) [
        XL=ABS(XYL2-TEMP4);
        XR=ABS(RMAX_CM(ICM_$SYNCVMLC)-XYL2);
       ]
      DIST=MIN(DIST,XL,XR);
     ]
    ELSE ["rounded leaf end"
      TEMP1=ABS(SQRT((Z(NP)-(ZMIN_$SYNCVMLC+ZTHICK_$SYNCVMLC/2))**2+
                     (XYL2-(LEAFB_$SYNCVMLC(LEAFIS)-LEAFRADIUS_$SYNCVMLC))**2)-
                      LEAFRADIUS_$SYNCVMLC);
      TEMP2=ABS(SQRT((Z(NP)-(ZMIN_$SYNCVMLC+ZTHICK_$SYNCVMLC/2))**2+
                     (XYL2-(LEAFA_$SYNCVMLC(LEAFIS)+LEAFRADIUS_$SYNCVMLC))**2)-
                     LEAFRADIUS_$SYNCVMLC);
      HOLE=HOLEPOS_$SYNCVMLC(LEAFIS);
      TEMP3=LEAFB_$SYNCVMLC(LEAFIS)-HOLE;
      TEMP4=LEAFA_$SYNCVMLC(LEAFIS)+HOLE;
      IF(NX=1) [
        XL=TEMP1;
        XR=TEMP2;
        ]
      IF(NX=3) [
        XL=ABS(XYL2-TEMP3);
        XR=TEMP1;
       ]
      IF(NX=2) [
        XL=ABS(-RMAX_CM(ICM_$SYNCVMLC)-XYL2);
        XR=ABS(XYL2-TEMP3);
       ]
      IF(NX=4) [
        XL=TEMP2;
        XR=ABS(TEMP4-XYL2);
       ]
      IF(NX=5) [
        XL=ABS(XYL2-TEMP4);
        XR=ABS(RMAX_CM(ICM_$SYNCVMLC)-XYL2);
       ]
      DIST=MIN(DIST,XL,XR);
    ]
IF(DIST<0) [OUTPUT LEAFIS,NX,NY,NZ; ('negative dist: ',4I3);]
 ]
ELSE[ "beyond outer edges of leaf bank"
  IF(XYL1 <= SURPARA1_$SYNCVMLC(1,1)*(Z(NP)-ZFOCUS_$SYNCVMLC(1)))[
      TEMP1=SURPARA1_$SYNCVMLC(1,1);
      TEMP1=ABS((TEMP1*(Z(NP)-ZFOCUS_$SYNCVMLC(1))-XYL1)/
                 SQRT(1+TEMP1**2));
    ]
  ELSEIF(XYL1 >= SURPARA1_$SYNCVMLC(TOT_LEAF_$SYNCVMLC,7)*
                   (Z(NP)-ZFOCUS_$SYNCVMLC(1)))[
      TEMP1=SURPARA1_$SYNCVMLC(TOT_LEAF_$SYNCVMLC,7);
      TEMP1=ABS((TEMP1*(Z(NP)-ZFOCUS_$SYNCVMLC(1))-XYL1)/
                 SQRT(1+TEMP1**2));
    ]
  DIST=MIN(Z(NP)-ZMIN_$SYNCVMLC,ZMAX_$SYNCVMLC-Z(NP),TEMP1);
 ]
 ]
 ];
 DIST_sngl=SNGL(DIST);
RETURN;
END; "End of subroutine HOWNEAR_$SYNCVMLC(Rev 1.4)"
"************************************************************************"
"End of SYNCVMLC_cm.mortran (Rev 1.4)"



OUTPUT LEAFWIDTH_$SYNCVMLC(3), WTONGUE_$SYNCVMLC(3), WGROOVE_$SYNCVMLC(3),
       WTIP_$SYNCVMLC(3), WRAILTOP_$SYNCVMLC(3), WRAILBOT_$SYNCVMLC(3),
       ZTIP_$SYNCVMLC(3), ZLEAF_$SYNCVMLC(3), ZTONGUE_$SYNCVMLC(3),
      ZGROOVE_$SYNCVMLC(3), ZHOLETOP_$SYNCVMLC(3), ZHOLEBOT_$SYNCVMLC(3),
      HOLEPOS_ISO_$SYNCVMLC, ZRAILTOP_$SYNCVMLC(3), ZRAILBOT_$SYNCVMLC(3);
      (15F15.5);
"now do checks on input dimensions"
IF(WGROOVE_$SYNCVMLC(3)>LEAFWIDTH_$SYNCVMLC(3))[
   OUTPUT ICM_$SYNCVMLC;
   (//' ***ERROR IN CM ',I3,' ($SYNCVMLC) in ISOCENTER leaf definition:'/
      ' WGROOVE_$SYNCVMLC(3)>LEAFWIDTH_$SYNCVMLC(3).  Groove too wide.'//);
         IERR_GEOM(ICM_$SYNCVMLC)=IERR_GEOM(ICM_$SYNCVMLC)+1;
]
IF(WTIP_$SYNCVMLC(3)>LEAFWIDTH_$SYNCVMLC(3)-WGROOVE_$SYNCVMLC(3))[
   OUTPUT ICM_$SYNCVMLC;
   (//' ***ERROR IN CM ',I3,' ($SYNCVMLC) in ISOCENTER leaf definition:'/
      ' WTIP_$SYNCVMLC(3)>LEAFWIDTH_$SYNCVMLC(3)-WGROOVE_$SYNCVMLC(3).'/
      ' Tip too wide.'//);
         IERR_GEOM(ICM_$SYNCVMLC)=IERR_GEOM(ICM_$SYNCVMLC)+1;
]
IF(WRAILTOP_$SYNCVMLC(3)>LEAFWIDTH_$SYNCVMLC(3)+WTONGUE_$SYNCVMLC(3)-
   WGROOVE_$SYNCVMLC(3))[
   OUTPUT ICM_$SYNCVMLC;
   (//' ***ERROR IN CM ',I3,' ($SYNCVMLC) in ISOCENTER leaf definition:'/
      ' WRAILTOP_$SYNCVMLC(3)>LEAFWIDTH_$SYNCVMLC(3)+WTONGUE_$SYNCVMLC(3)-'/
      '                  WGROOVE_$SYNCVMLC(3).  Top of rail too wide.'//);
         IERR_GEOM(ICM_$SYNCVMLC)=IERR_GEOM(ICM_$SYNCVMLC)+1;
]
IF(WRAILTOP_$SYNCVMLC(3)<WTONGUE_$SYNCVMLC(3)+WTIP_$SYNCVMLC(3))[
   OUTPUT ICM_$SYNCVMLC;
   (//' ***ERROR IN CM ',I3,' ($SYNCVMLC) in ISOCENTER leaf definition:'/
      ' WRAILTOP_$SYNCVMLC(3)<WTONGUE_$SYNCVMLC(3)+WTIP_$SYNCVMLC(3).'/
      ' Top of rail too narrow.'//);
         IERR_GEOM(ICM_$SYNCVMLC)=IERR_GEOM(ICM_$SYNCVMLC)+1;
]
IF(WRAILBOT_$SYNCVMLC(3)>WRAILTOP_$SYNCVMLC(3))[
   OUTPUT ICM_$SYNCVMLC;
   (//' ***ERROR IN CM ',I3,' ($SYNCVMLC) in ISOCENTER leaf definition:'/
' WRAILBOT_$SYNCVMLC(3)>WRAILTOP_$SYNCVMLC(3).  Bottom of rail too wide.'//);
         IERR_GEOM(ICM_$SYNCVMLC)=IERR_GEOM(ICM_$SYNCVMLC)+1;
]
IF(WRAILBOT_$SYNCVMLC(3)<WRAILTOP_$SYNCVMLC(3)-WTONGUE_$SYNCVMLC(3))[
   OUTPUT ICM_$SYNCVMLC;
   (//' ***ERROR IN CM ',I3,' ($SYNCVMLC) in ISOCENTER leaf definition:'/
      ' WRAILBOT_$SYNCVMLC(3)<WRAILTOP_$SYNCVMLC(3)-WTONGUE_$SYNCVMLC(3).'/
      ' Bottom of rail too narrow.'//);
         IERR_GEOM(ICM_$SYNCVMLC)=IERR_GEOM(ICM_$SYNCVMLC)+1;
]
IF(ZTIP_$SYNCVMLC(3)<ZMIN_$SYNCVMLC)[
   OUTPUT ICM_$SYNCVMLC;
   (//' ***ERROR IN CM ',I3,' ($SYNCVMLC) in ISOCENTER leaf definition:'/
      ' ZTIP_$SYNCVMLC(3)<ZMIN_$SYNCVMLC.'//);
         IERR_GEOM(ICM_$SYNCVMLC)=IERR_GEOM(ICM_$SYNCVMLC)+1;
]
IF(ZLEAF_$SYNCVMLC(3)<ZTIP_$SYNCVMLC(3))[
   OUTPUT ICM_$SYNCVMLC;
   (//' ***ERROR IN CM ',I3,' ($SYNCVMLC) in ISOCENTER leaf definition:'/
      ' ZLEAF_$SYNCVMLC(3)<ZTIP_$SYNCVMLC(3).'//);
         IERR_GEOM(ICM_$SYNCVMLC)=IERR_GEOM(ICM_$SYNCVMLC)+1;
]
IF(ZTONGUE_$SYNCVMLC(3)<ZLEAF_$SYNCVMLC(3))[
   OUTPUT ICM_$SYNCVMLC;
   (//' ***ERROR IN CM ',I3,' ($SYNCVMLC) in ISOCENTER leaf definition:'/
      ' ZTONGUE_$SYNCVMLC(3)<ZLEAF_$SYNCVMLC(3).'//);
         IERR_GEOM(ICM_$SYNCVMLC)=IERR_GEOM(ICM_$SYNCVMLC)+1;
]
IF(ZGROOVE_$SYNCVMLC(3)<ZTONGUE_$SYNCVMLC(3))[
   OUTPUT ICM_$SYNCVMLC;
   (//' ***ERROR IN CM ',I3,' ($SYNCVMLC) in ISOCENTER leaf definition:'/
      ' ZGROOVE_$SYNCVMLC(3)<ZTONGUE_$SYNCVMLC(3).'//);
         IERR_GEOM(ICM_$SYNCVMLC)=IERR_GEOM(ICM_$SYNCVMLC)+1;
]
IF(ZHOLETOP_$SYNCVMLC(3)<ZGROOVE_$SYNCVMLC(3))[
   OUTPUT ICM_$SYNCVMLC;
   (//' ***ERROR IN CM ',I3,' ($SYNCVMLC) in ISOCENTER leaf definition:'/
      ' ZHOLETOP_$SYNCVMLC(3)<ZGROOVE_$SYNCVMLC(3).'//);
         IERR_GEOM(ICM_$SYNCVMLC)=IERR_GEOM(ICM_$SYNCVMLC)+1;
]
IF(ZHOLEBOT_$SYNCVMLC(3)<ZHOLETOP_$SYNCVMLC(3))[
   OUTPUT ICM_$SYNCVMLC;
   (//' ***ERROR IN CM ',I3,' ($SYNCVMLC) in ISOCENTER leaf definition:'/
      ' ZHOLEBOT_$SYNCVMLC(3)<ZHOLETOP_$SYNCVMLC(3).'//);
         IERR_GEOM(ICM_$SYNCVMLC)=IERR_GEOM(ICM_$SYNCVMLC)+1;
]
IF(ZRAILTOP_$SYNCVMLC(3)<ZHOLEBOT_$SYNCVMLC(3))[
   OUTPUT ICM_$SYNCVMLC;
   (//' ***ERROR IN CM ',I3,' ($SYNCVMLC) in ISOCENTER leaf definition:'/
      ' ZRAILTOP_$SYNCVMLC(3)<ZHOLEBOT_$SYNCVMLC(3).'//);
         IERR_GEOM(ICM_$SYNCVMLC)=IERR_GEOM(ICM_$SYNCVMLC)+1;
]
IF(ZRAILBOT_$SYNCVMLC(3)<ZRAILTOP_$SYNCVMLC(3))[
   OUTPUT ICM_$SYNCVMLC;
   (//' ***ERROR IN CM ',I3,' ($SYNCVMLC) in ISOCENTER leaf definition:'/
      ' ZRAILBOT_$SYNCVMLC(3)<ZRAILTOP_$SYNCVMLC(3).'//);
         IERR_GEOM(ICM_$SYNCVMLC)=IERR_GEOM(ICM_$SYNCVMLC)+1;
]
IF(ZRAILBOT_$SYNCVMLC(3)>ZMIN_$SYNCVMLC+ZTHICK_$SYNCVMLC)[
   OUTPUT ICM_$SYNCVMLC;
   (//' ***ERROR IN CM ',I3,' ($SYNCVMLC) in ISOCENTER leaf definition:'/
      ' ZRAILBOT_$SYNCVMLC(3)>ZMIN_$SYNCVMLC+ZTHICK_$SYNCVMLC.'//);
         IERR_GEOM(ICM_$SYNCVMLC)=IERR_GEOM(ICM_$SYNCVMLC)+1;
]

"now check dimensions between leaf types"
IF(ZTONGUE_$SYNCVMLC(3)<=ZGROOVE_$SYNCVMLC(2))[
   OUTPUT ICM_$SYNCVMLC;
   (//' ***ERROR IN CM ',I3,' ($SYNCVMLC) in leaf definition:'/
      ' ZTONGUE_$SYNCVMLC(3)<=ZGROOVE_$SYNCVMLC(2).'/
      ' ISOCENTER/TARGET leaves will not fit together.'//);
       IERR_GEOM(ICM_$SYNCVMLC)=IERR_GEOM(ICM_$SYNCVMLC)+1;
]
IF(ZTONGUE_$SYNCVMLC(2)>=ZGROOVE_$SYNCVMLC(3))[
   OUTPUT ICM_$SYNCVMLC;
   (//' ***ERROR IN CM ',I3,' ($SYNCVMLC) in leaf definition:'/
      ' ZTONGUE_$SYNCVMLC(2)>=ZGROOVE_$SYNCVMLC(3).'/
      ' ISOCENTER/TARGET leaves will not fit together.'//);
       IERR_GEOM(ICM_$SYNCVMLC)=IERR_GEOM(ICM_$SYNCVMLC)+1;
]
IF(ZGROOVE_$SYNCVMLC(3)<=ZTONGUE_$SYNCVMLC(1))[
   OUTPUT ICM_$SYNCVMLC;
   (//' ***ERROR IN CM ',I3,' ($SYNCVMLC) in leaf definition:'/
      ' ZGROOVE_$SYNCVMLC(3)<=ZTONGUE_$SYNCVMLC(1).'/
      ' ISOCENTER leaf will not fit with'/
      ' FULL leaf if ISOCENTER leaf is on -X or -Y side.'//);
       IERR_GEOM(ICM_$SYNCVMLC)=IERR_GEOM(ICM_$SYNCVMLC)+1;
]
IF(ZTONGUE_$SYNCVMLC(2)>=ZGROOVE_$SYNCVMLC(1))[
   OUTPUT ICM_$SYNCVMLC;
   (//' ***ERROR IN CM ',I3,' ($SYNCVMLC) in leaf definition:'/
      ' ZTONGUE_$SYNCVMLC(2)>=ZGROOVE_$SYNCVMLC(1).'/
      ' TARGET leaf will not fit with'/
      ' FULL leaf if FULL leaf is on -X or -Y side.'//);
       IERR_GEOM(ICM_$SYNCVMLC)=IERR_GEOM(ICM_$SYNCVMLC)+1;
]

TOT_LEAF_$SYNCVMLC=0;
K=0;
OUTPUT;(' ');
DO I=1,NGROUP_$SYNCVMLC [
   OUTPUT I;
   (' Group ',I3,':'/
    '   No. of leaves, leaf type (1. FULL leaves--the default, '/
    '   2. TARGET/ISOCENTER pairs with TARGET leaf on -X or -Y side)'/
    '   :',$);
   MINPUT ($SYNCVMLC) NUM_LEAF_$SYNCVMLC(I), LEAFTYPE; (2I5);
   OUTPUT NUM_LEAF_$SYNCVMLC(I), LEAFTYPE; (2I5);

   TOT_LEAF_$SYNCVMLC=TOT_LEAF_$SYNCVMLC+NUM_LEAF_$SYNCVMLC(I);

   IF(LEAFTYPE<=0|LEAFTYPE>2)[
     LEAFTYPE=1;
     OUTPUT ICM_$SYNCVMLC,I;
         (//' ***WARNING IN CM ',I3,' ($SYNCVMLC)'/
            ' Leaf type for group ',I3,' not recognized.'/
            ' Will default to 1.'//);
   ]
   IF(MOD(NUM_LEAF_$SYNCVMLC(I),2)~=0 & (LEAFTYPE=2 |
     LEAFTYPE=3))[
      OUTPUT ICM_$SYNCVMLC,I;
         (//' ***ERROR IN CM ',I3,' ($SYNCVMLC)'/
            ' Leaf group ',I3,' consists of TARGET/ISOCENTER pairs but'/
            ' does not have an even number of leaves.'//);
     IERR_GEOM(ICM_$SYNCVMLC)=IERR_GEOM(ICM_$SYNCVMLC)+1;
   ]
   "now set LEAFTYPE for each leaf"
   DO J=1,NUM_LEAF_$SYNCVMLC(I)[
      K=K+1;
      IF(LEAFTYPE=1)[
         LEAFTYPE_$SYNCVMLC(K)=1;
      ]
      ELSE["target/isocenter pair"
        IF(MOD(J,2)~=0)["target leaf"
           LEAFTYPE_$SYNCVMLC(K)=2;
        ]
        ELSE[
           LEAFTYPE_$SYNCVMLC(K)=3;
        ]
      ]
   ]
]
;
IF(TOT_LEAF_$SYNCVMLC>$MAXLEAF) [
    TOT_LEAF_$SYNCVMLC=$MAXLEAF;
    OUTPUT ICM_$SYNCVMLC,TOT_LEAF_$SYNCVMLC;
            (//' ***ERROR IN CM ',I3,' ($SYNCVMLC)'/
             ' Total # of leaves > max allowed'/
             ' Total # of leaves reduced to ',I3,' for now'//);
    IERR_GEOM(ICM_$SYNCVMLC)=IERR_GEOM(ICM_$SYNCVMLC)+1;
 ]
;
IF(TOT_LEAF_$SYNCVMLC<=0) [
    TOT_LEAF_$SYNCVMLC=1;
    OUTPUT ICM_$SYNCVMLC,TOT_LEAF_$SYNCVMLC;
            (//' ***ERROR IN CM ',I3,' ($SYNCVMLC)'/
             ' Total # of leaves <= 0'/
             ' Total # of leaves set to ',I3,' for now'//);
    IERR_GEOM(ICM_$SYNCVMLC)=IERR_GEOM(ICM_$SYNCVMLC)+1;
 ];

" III. START POSITION OF LEAVES"
" ============================="
;
OUTPUT ZMIN_$SYNCVMLC; (/' Input the starting X (ORIENT_$SYNCVMLC=0) or '/
         ' Y (ORIENT_$SYNCVMLC=1) position at z = ',F15.0, ' cm : ', $);
MINPUT ($SYNCVMLC) START_$SYNCVMLC; (F15.0);
OUTPUT START_$SYNCVMLC; (F12.5);

" Checking the validity of user-input"

IF(ABS(START_$SYNCVMLC)-RMAX_CM(ICM_$SYNCVMLC)>1.E-5) [
  OUTPUT ICM_$SYNCVMLC;
   (//'***ERROR IN CM ',I4,' ($SYNCVMLC)'/
   ' START POSITION EXCEEDS CM BOUNDARY'//);
   IERR_GEOM(ICM_$SYNCVMLC)=IERR_GEOM(ICM_$SYNCVMLC)+1;
 ];

"IV. INTER-LEAF AIR GAP "
"====================== "

OUTPUT; (/'Input the inter-leaf air gap(>=0.0) : ',$);
  "leaf gap defined at ZMIN"
MINPUT ($SYNCVMLC) LEAFGAP_$SYNCVMLC; (F15.0);
OUTPUT LEAFGAP_$SYNCVMLC; (F12.5);

IF (LEAFGAP_$SYNCVMLC<0.0) [
      LEAFGAP_$SYNCVMLC=0;
      OUTPUT ICM_$SYNCVMLC;
         (//'***ERROR IN CM ',I4,' ($SYNCVMLC)'/
         ' Inter-leaf air gap is negative - reset to 0   '//);
         IERR_GEOM(ICM_$SYNCVMLC)=IERR_GEOM(ICM_$SYNCVMLC)+1;
      ];

"Check that there are no unblocked areas between adjacent leaves"
IF(LEAFGAP_$SYNCVMLC>WTONGUE_$SYNCVMLC(1))[
        LEAFGAP_$SYNCVMLC = WTONGUE_$SYNCVMLC(1);
        OUTPUT ICM_$SYNCVMLC;
        (//'**ERROR IN CM ',I4,' ($SYNCVMLC)'/
       ' Width of inter-leaf airgap exceeds that of the FULL leaf tongue.',/
       ' Reset to be equal to the width of the tongue. '//);
         IERR_GEOM(ICM_$SYNCVMLC)=IERR_GEOM(ICM_$SYNCVMLC)+1;
]
IF(LEAFGAP_$SYNCVMLC>WTONGUE_$SYNCVMLC(3))[
        LEAFGAP_$SYNCVMLC = WTONGUE_$SYNCVMLC(3);
        OUTPUT ICM_$SYNCVMLC;
        (//'**ERROR IN CM ',I4,' ($SYNCVMLC)'/
   ' Width of inter-leaf airgap exceeds that of the ISOCENTER leaf tongue.',/
   ' Reset to be equal to the width of the tongue. '//);
        IERR_GEOM(ICM_$SYNCVMLC)=IERR_GEOM(ICM_$SYNCVMLC)+1;
]
IF(LEAFGAP_$SYNCVMLC>WTONGUE_$SYNCVMLC(2))[
        LEAFGAP_$SYNCVMLC = WTONGUE_$SYNCVMLC(2);
        OUTPUT ICM_$SYNCVMLC;
        (//'**ERROR IN CM ',I4,' ($SYNCVMLC)'/
   ' Width of inter-leaf airgap exceeds that of the TARGET leaf tongue.',/
   ' Reset to be equal to the width of the tongue. '//);
        IERR_GEOM(ICM_$SYNCVMLC)=IERR_GEOM(ICM_$SYNCVMLC)+1;
]

"check leaf gap between adjacent tongue and groove"
IF(WGROOVE_$SYNCVMLC(1)+LEAFGAP_$SYNCVMLC-WTONGUE_$SYNCVMLC(1)<0.0)[
    OUTPUT ICM_$SYNCVMLC;
     (//' ***ERROR IN CM ',I4,' ($SYNCVMLC)'/
        ' Tongue in FULL leaf too wide for groove in FULL leaf. '//);
     IERR_GEOM(ICM_$SYNCVMLC)=IERR_GEOM(ICM_$SYNCVMLC)+1;
]
IF(WGROOVE_$SYNCVMLC(2)+LEAFGAP_$SYNCVMLC-WTONGUE_$SYNCVMLC(3)<0.0)[
    OUTPUT ICM_$SYNCVMLC;
     (//' ***ERROR IN CM ',I4,' ($SYNCVMLC)'/
        ' Tongue in ISOCENTER leaf too wide for groove in TARGET leaf. '//);
     IERR_GEOM(ICM_$SYNCVMLC)=IERR_GEOM(ICM_$SYNCVMLC)+1;
]
IF(WGROOVE_$SYNCVMLC(1)+LEAFGAP_$SYNCVMLC-WTONGUE_$SYNCVMLC(2)<0.0)[
    OUTPUT ICM_$SYNCVMLC;
     (//' ***ERROR IN CM ',I4,' ($SYNCVMLC)'/
        ' Tongue in TARGET leaf too wide for groove in FULL leaf. '//);
     IERR_GEOM(ICM_$SYNCVMLC)=IERR_GEOM(ICM_$SYNCVMLC)+1;
]
IF(WGROOVE_$SYNCVMLC(3)+LEAFGAP_$SYNCVMLC-WTONGUE_$SYNCVMLC(1)<0.0)[
    OUTPUT ICM_$SYNCVMLC;
     (//' ***ERROR IN CM ',I4,' ($SYNCVMLC)'/
        ' Tongue in FULL leaf too wide for groove in ISOCENTER leaf. '//);
     IERR_GEOM(ICM_$SYNCVMLC)=IERR_GEOM(ICM_$SYNCVMLC)+1;
]

"V. TYPE OF LEAF ENDS - ROUNDED VS FLAT DIVERGENT "
"================================================ "

OUTPUT; (/'Input the type of leaf end desired : ',/,
          '   0 --- Rounded leaf end or ',/
          '   1 --- Focused leaf end  ');
OUTPUT; ('Input your choice : ', $);
MINPUT ($SYNCVMLC) ENDTYPE_$SYNCVMLC; (I5);
OUTPUT ENDTYPE_$SYNCVMLC; (I5/);
;
IF(ENDTYPE_$SYNCVMLC=1) [
   OUTPUT; (' Input the z focus of the leaf ends : ',$);
   MINPUT ($SYNCVMLC) ZFOCUS_$SYNCVMLC(2); (F15.0);
   OUTPUT ZFOCUS_$SYNCVMLC(2);(F12.5/);

   IF(ABS(ZFOCUS_$SYNCVMLC(2)-ZMIN_$SYNCVMLC)<1.E-5) [
      ZFOCUS_$SYNCVMLC(2)=ZMIN_$SYNCVMLC-1.E-4;
      OUTPUT ICM_$SYNCVMLC,ZFOCUS_$SYNCVMLC(2);
          (//' ***ERROR IN CM ',I4,' ($SYNCVMLC)'/
           ' ZFOCUS(2) cannot be equal to ZMIN_$SYNCVMLC'/
           ' ZFOCUS(2) reset to ',F15.5,' cm for now'//);
      IERR_GEOM(ICM_$SYNCVMLC)=IERR_GEOM(ICM_$SYNCVMLC)+1;
     ]
   ELSEIF(ZFOCUS_$SYNCVMLC(2)>ZMIN_$SYNCVMLC &
          ZFOCUS_$SYNCVMLC(2)<ZMAX_$SYNCVMLC)[
        ZFOCUS_$SYNCVMLC(2)=ZMAX_$SYNCVMLC;
        OUTPUT ICM_$SYNCVMLC,ZFOCUS_$SYNCVMLC(2);
          (//' ***ERROR IN CM ',I4,' ($SYNCVMLC)'/
          ' ZFOCUS(2) is between ZMIN_$SYNCVMLC and ZMAX_$SYNCVMLC'/
          ' This will cause leaf ends to overlap'/
          ' ZFOCUS(2) reset to ',F15.5,' cm for now'//);
       IERR_GEOM(ICM_$SYNCVMLC)=IERR_GEOM(ICM_$SYNCVMLC)+1;
     ];
 ]
ELSE  [ "Defaults to rounded ends "

   OUTPUT (ZTHICK_$SYNCVMLC/2);
          (' Input the radius >= ', F12.5, ' cm ', /
          ' of the leaf ends : ',$);

   MINPUT ($SYNCVMLC) LEAFRADIUS_$SYNCVMLC; (F15.0);
   OUTPUT LEAFRADIUS_$SYNCVMLC; (F15.0/);

   IF((LEAFRADIUS_$SYNCVMLC < (0.5*ZTHICK_$SYNCVMLC)) |
      (LEAFRADIUS_$SYNCVMLC < 0.0)) [

" This is done to ensure that the leaf end is rounded all the"
" way from zmin_$syncvmlc to zmax_$syncvmlc. "
" Otherwise the curvature will fall short."

     LEAFRADIUS_$SYNCVMLC=0.5*ZTHICK_$SYNCVMLC;
     OUTPUT ICM_$SYNCVMLC,LEAFRADIUS_$SYNCVMLC;
      (//' ***ERROR IN CM ',I4,' ($SYNCVMLC)'/
      ' LEAFRADIUS_$SYNCVMLC is less than minimum acceptable'/
      ' RESET TO ', F15.5, ' cm for now'//);
     IERR_GEOM(ICM_$SYNCVMLC)=IERR_GEOM(ICM_$SYNCVMLC)+1;
    ]
    IF(HOLEPOS_FULL_$SYNCVMLC>=LEAFRADIUS_$SYNCVMLC) [
      HOLEPOS_FULL_$SYNCVMLC=LEAFRADIUS_$SYNCVMLC-0.1;
      OUTPUT ICM_$SYNCVMLC,LEAFRADIUS_$SYNCVMLC;
      (//'***ERROR IN CM ',I4,' ($SYNCVMLC)'/
      ' Hole position in FULL leaves is greater than leaf'/
      ' radius. Reset to ',F12.5,' cm for now'//);
      IERR_GEOM(ICM_$SYNCVMLC)=IERR_GEOM(ICM_$SYNCVMLC)+1;
    ]
    IF(HOLEPOS_ISO_$SYNCVMLC>=LEAFRADIUS_$SYNCVMLC) [
      HOLEPOS_ISO_$SYNCVMLC=LEAFRADIUS_$SYNCVMLC-0.1;
      OUTPUT ICM_$SYNCVMLC,LEAFRADIUS_$SYNCVMLC;
      (//'***ERROR IN CM ',I4,' ($SYNCVMLC)'/
      ' Hole position in ISOCENTER leaves is greater than leaf'/
      ' radius. Reset to ',F12.5,' cm for now'//);
      IERR_GEOM(ICM_$SYNCVMLC)=IERR_GEOM(ICM_$SYNCVMLC)+1;
    ]
    IF(HOLEPOS_TAR_$SYNCVMLC>=LEAFRADIUS_$SYNCVMLC) [
      HOLEPOS_TAR_$SYNCVMLC=LEAFRADIUS_$SYNCVMLC-0.1;
      OUTPUT ICM_$SYNCVMLC,LEAFRADIUS_$SYNCVMLC;
      (//'***ERROR IN CM ',I4,' ($SYNCVMLC)'/
      ' Hole position in TARGET leaves is greater than leaf'/
      ' radius. Reset to ',F12.5,' cm for now'//);
      IERR_GEOM(ICM_$SYNCVMLC)=IERR_GEOM(ICM_$SYNCVMLC)+1;
    ]
 ]; " End of checking the leaf end ;


"VI. FOCUS FOR DIVERGENT LEAF SIDES "
"==================================== "

OUTPUT; (' Input the Z focus point of the leaf sides: ',$);
MINPUT ($SYNCVMLC) ZFOCUS_$SYNCVMLC(1); (F15.0);
OUTPUT ZFOCUS_$SYNCVMLC(1); (F12.5/);
IF(ABS(ZFOCUS_$SYNCVMLC(1)-ZMIN_$SYNCVMLC)<1.E-5) [
   ZFOCUS_$SYNCVMLC(1)=ZMIN_$SYNCVMLC-1.E-4;
   OUTPUT ICM_$SYNCVMLC,ZFOCUS_$SYNCVMLC(1);
      (//' ***ERROR IN CM ',I4,' ($SYNCVMLC)'/
       ' ZFOCUS(1) cannot be equal to ZMIN_$SYNCVMLC'/
       ' ZFOCUS(1) reset to ',F15.5,' cm for now'//);
   IERR_GEOM(ICM_$SYNCVMLC)=IERR_GEOM(ICM_$SYNCVMLC)+1;
 ]
ELSEIF(ZFOCUS_$SYNCVMLC(1)>ZMIN_$SYNCVMLC &
    ZFOCUS_$SYNCVMLC(1)<ZMAX_$SYNCVMLC) [
   ZFOCUS_$SYNCVMLC(1)=ZMAX_$SYNCVMLC;
   OUTPUT ICM_$SYNCVMLC,ZFOCUS_$SYNCVMLC(1);
      (//' ***ERROR IN CM ',I4,' ($SYNCVMLC)'/
       ' ZFOCUS(1) is between ZMIN_$SYNCVMLC and ZMAX_$SYNCVMLC'/
       ' This will cause leaf sides to overlap'/
       ' ZFOCUS(1) reset to ',F15.5,' cm for now'//);
   IERR_GEOM(ICM_$SYNCVMLC)=IERR_GEOM(ICM_$SYNCVMLC)+1;
 ];

"VII. CALCULATE THE Z-AXIS COORDINATES OF DIFFERENT SUB-REGIONS "
"============================================================= "
"this is done on a leaf-by-leaf basis"
I=0;
DO J=1,NGROUP_$SYNCVMLC[
  DO K=1,NUM_LEAF_$SYNCVMLC(J)[
    I=I+1;
    ZREG_$SYNCVMLC(I,1)=ZMIN_$SYNCVMLC;
    IF(LEAFTYPE_$SYNCVMLC(I)=1)["full leaf"
      ZREG_$SYNCVMLC(I,2)=ZTIP_$SYNCVMLC(1);
      ZREG_$SYNCVMLC(I,3)=ZLEAF_$SYNCVMLC(1);
      ZREG_$SYNCVMLC(I,4)=ZTONGUE_$SYNCVMLC(1);
      ZREG_$SYNCVMLC(I,5)=ZGROOVE_$SYNCVMLC(1);
      ZREG_$SYNCVMLC(I,6)=ZHOLETOP_$SYNCVMLC(1);
      ZREG_$SYNCVMLC(I,7)=ZHOLEBOT_$SYNCVMLC(1);
      ZREG_$SYNCVMLC(I,8)=ZRAILTOP_$SYNCVMLC(1);
      ZREG_$SYNCVMLC(I,9)=ZRAILBOT_$SYNCVMLC(1);
      ZREG_$SYNCVMLC(I,10)=ZREG_$SYNCVMLC(I,1)+ZTHICK_$SYNCVMLC;
      HOLEPOS_$SYNCVMLC(I)=HOLEPOS_FULL_$SYNCVMLC;"do this here"
    ]
    ELSEIF(LEAFTYPE_$SYNCVMLC(I)=2)["target leaf"
      ZREG_$SYNCVMLC(I,2)=ZRAILTOP_$SYNCVMLC(2);
      ZREG_$SYNCVMLC(I,3)=ZRAILBOT_$SYNCVMLC(2);
      ZREG_$SYNCVMLC(I,4)=ZHOLETOP_$SYNCVMLC(2);
      ZREG_$SYNCVMLC(I,5)=ZHOLEBOT_$SYNCVMLC(2);
      ZREG_$SYNCVMLC(I,6)=ZTONGUE_$SYNCVMLC(2);
      ZREG_$SYNCVMLC(I,7)=ZGROOVE_$SYNCVMLC(2);
      ZREG_$SYNCVMLC(I,8)=ZLEAF_$SYNCVMLC(2);
      ZREG_$SYNCVMLC(I,9)=ZTIP_$SYNCVMLC(2);
      ZREG_$SYNCVMLC(I,10)=ZREG_$SYNCVMLC(I,1)+ZTHICK_$SYNCVMLC;
      HOLEPOS_$SYNCVMLC(I)=HOLEPOS_TAR_$SYNCVMLC;
    ]
    ELSE["isocenter"
      ZREG_$SYNCVMLC(I,2)=ZTIP_$SYNCVMLC(3);
      ZREG_$SYNCVMLC(I,3)=ZLEAF_$SYNCVMLC(3);
      ZREG_$SYNCVMLC(I,4)=ZTONGUE_$SYNCVMLC(3);
      ZREG_$SYNCVMLC(I,5)=ZGROOVE_$SYNCVMLC(3);
      ZREG_$SYNCVMLC(I,6)=ZHOLETOP_$SYNCVMLC(3);
      ZREG_$SYNCVMLC(I,7)=ZHOLEBOT_$SYNCVMLC(3);
      ZREG_$SYNCVMLC(I,8)=ZRAILTOP_$SYNCVMLC(3);
      ZREG_$SYNCVMLC(I,9)=ZRAILBOT_$SYNCVMLC(3);
      ZREG_$SYNCVMLC(I,10)=ZREG_$SYNCVMLC(I,1)+ZTHICK_$SYNCVMLC;
      HOLEPOS_$SYNCVMLC(I)=HOLEPOS_ISO_$SYNCVMLC;
    ]
  ]
];

"VIII. CALC PARAMETERS IN DIRECTION PERPENDICULAR TO LEAF ORIENTATION
"==================================================================

TEMP1 = (ZMIN_$SYNCVMLC-ZFOCUS_$SYNCVMLC(1));

I=0;
IF(TOT_LEAF_$SYNCVMLC>0) [
  DO L = 1,NGROUP_$SYNCVMLC[
   DO K = 1,NUM_LEAF_$SYNCVMLC(L) [
     I=I+1;
     IF(I=1)[
       YREG_$SYNCVMLC(I,1)=START_$SYNCVMLC;
     ]
     ELSEIF(LEAFTYPE_$SYNCVMLC(I)=1)[
       YREG_$SYNCVMLC(I,1)=YREG_$SYNCVMLC(I-1,7)+LEAFGAP_$SYNCVMLC-
                          WTONGUE_$SYNCVMLC(1);
     ]
     ELSEIF(LEAFTYPE_$SYNCVMLC(I)=2)["target leaf"
         YREG_$SYNCVMLC(I,1)=YREG_$SYNCVMLC(I-1,7)+LEAFGAP_$SYNCVMLC-
                          WTONGUE_$SYNCVMLC(2);
     ]
     ELSE["isocenter leaf"
         YREG_$SYNCVMLC(I,1)=YREG_$SYNCVMLC(I-1,7)+LEAFGAP_$SYNCVMLC-
                          WTONGUE_$SYNCVMLC(3);
     ]
     "we have now defined YREG_$SYNCVMLC(I,1)"
     IF(LEAFTYPE_$SYNCVMLC(I)=1)["full leaf"
       YREG_$SYNCVMLC(I,2)=YREG_$SYNCVMLC(I,1)+WTONGUE_$SYNCVMLC(1);
       YREG_$SYNCVMLC(I,3)=YREG_$SYNCVMLC(I,1)+WTIP_$SYNCVMLC(1);
       YREG_$SYNCVMLC(I,4)=YREG_$SYNCVMLC(I,2)+WRAILTOP_$SYNCVMLC(1)-
                          WRAILBOT_$SYNCVMLC(1);
       YREG_$SYNCVMLC(I,5)=YREG_$SYNCVMLC(I,2)+WRAILTOP_$SYNCVMLC(1);
       YREG_$SYNCVMLC(I,6)=YREG_$SYNCVMLC(I,2)+LEAFWIDTH_$SYNCVMLC(1)-
                          WGROOVE_$SYNCVMLC(1);
       YREG_$SYNCVMLC(I,7)=YREG_$SYNCVMLC(I,2)+LEAFWIDTH_$SYNCVMLC(1);
     ]
     ELSEIF(LEAFTYPE_$SYNCVMLC(I)=2)["target leaf"
       YREG_$SYNCVMLC(I,2)=YREG_$SYNCVMLC(I,1)+WTONGUE_$SYNCVMLC(2);
       YREG_$SYNCVMLC(I,3)=YREG_$SYNCVMLC(I,2)+LEAFWIDTH_$SYNCVMLC(2)-
                          WRAILBOT_$SYNCVMLC(2);
       YREG_$SYNCVMLC(I,4)=YREG_$SYNCVMLC(I,2)+LEAFWIDTH_$SYNCVMLC(2)-
                           WGROOVE_$SYNCVMLC(2)-WTIP_$SYNCVMLC(2);
       YREG_$SYNCVMLC(I,5)=YREG_$SYNCVMLC(I,2)+LEAFWIDTH_$SYNCVMLC(2)-
                          WGROOVE_$SYNCVMLC(2);
       YREG_$SYNCVMLC(I,6)=YREG_$SYNCVMLC(I,2)+LEAFWIDTH_$SYNCVMLC(2)-
                          WRAILBOT_$SYNCVMLC(2)+WRAILTOP_$SYNCVMLC(2);
       YREG_$SYNCVMLC(I,7)=YREG_$SYNCVMLC(I,2)+LEAFWIDTH_$SYNCVMLC(2);
     ]
     ELSE["isocenter leaf"
       YREG_$SYNCVMLC(I,2)=YREG_$SYNCVMLC(I,1)+WRAILTOP_$SYNCVMLC(3)-
                          WRAILBOT_$SYNCVMLC(3);
       YREG_$SYNCVMLC(I,3)=YREG_$SYNCVMLC(I,1)+WTONGUE_$SYNCVMLC(3);
       YREG_$SYNCVMLC(I,4)=YREG_$SYNCVMLC(I,3)+WTIP_$SYNCVMLC(3);
       YREG_$SYNCVMLC(I,5)=YREG_$SYNCVMLC(I,1)+WRAILTOP_$SYNCVMLC(3);
       YREG_$SYNCVMLC(I,6)=YREG_$SYNCVMLC(I,3)+LEAFWIDTH_$SYNCVMLC(3)-
                          WGROOVE_$SYNCVMLC(3);
       YREG_$SYNCVMLC(I,7)=YREG_$SYNCVMLC(I,3)+LEAFWIDTH_$SYNCVMLC(3);
     ]
     DO J = 1, 7 [
    SURPARA1_$SYNCVMLC(I,J)=YREG_$SYNCVMLC(I,J)/TEMP1;
            ];                "End of J Loop"
  ]; "End of K Loop"
 ]; "End of L loop"
];   "End of IF statement"

" Check to see if the SYNCVMLC will go outside the CM boundary "

TEMP1 = ABS((ZMAX_$SYNCVMLC-ZFOCUS_$SYNCVMLC(1))*SURPARA1_$SYNCVMLC(1,2));
TEMP2 = ABS((ZMAX_$SYNCVMLC-ZFOCUS_$SYNCVMLC(1))*
            SURPARA1_$SYNCVMLC(TOT_LEAF_$SYNCVMLC,6));

IF(TEMP1>RMAX_CM(ICM_$SYNCVMLC)|TEMP2>RMAX_CM(ICM_$SYNCVMLC)) [
   OUTPUT; (/' The SYNCVMLC is not contained within the CM boundaries', /
        ' Please check ZFOCUS_$SYNCVMLC(1) and RMAX_CM(ICM_$SYNCVMLC)'/);
   STOP;
  ];


"IX. INPUT THE COORDINATES NEG_$SYNCVMLC AND POS_$SYNCVMLC "
"===================================================== "
mlc_unit=70;
DO I=1, TOT_LEAF_$SYNCVMLC
[
    LEAFB_$SYNCVMLC(I)=0.0;LEAFA_$SYNCVMLC(I)=0.0;
]
IF(MODE_$SYNCVMLC=1|MODE_$SYNCVMLC=2)["dynamic or step-and-shoot leaf inputs"
  OUTPUT;(/' Input full name of file containing leaf opening data:'/);
  READ(i_input,'(A80)')mlc_file;
  OUTPUT mlc_file;(A80/);
  mlc_unit=egs_get_unit(mlc_unit);
  IF(mlc_unit<1)[
    $egs_fatal(*,
         'MLC leaf opening file: failed to get a free Fortran I/O unit');
  ]
  open(mlc_unit,file=mlc_file,status='old',err=:no-mlc-data-file:);
  read(mlc_unit,'(A80)') MLC_TITLE;
  read(mlc_unit,'(I10)') NFIELDS_$SYNCVMLC;
  OUTPUT NFIELDS_$SYNCVMLC; (I10);
  DO I=1,NFIELDS_$SYNCVMLC[
  "read in non-default leaf positions for each field"
     read(mlc_unit,'(F15.0)')INDEX_$SYNCVMLC(I);
     J=1;"counter for total no. of leaves input so far"
     LOOP[
       read(mlc_unit,'(2F15.0,I5)')NEG_$SYNCVMLC, POS_$SYNCVMLC, NUM_$SYNCVMLC;
       IF(NUM_$SYNCVMLC<=0) NUM_$SYNCVMLC=1;
       DO K=J,J+NUM_$SYNCVMLC-1[
         IF(K>TOT_LEAF_$SYNCVMLC) EXIT;
         LEAFNEG_$SYNCVMLC(K+(I-1)*TOT_LEAF_$SYNCVMLC)=NEG_$SYNCVMLC;
         LEAFPOS_$SYNCVMLC(K+(I-1)*TOT_LEAF_$SYNCVMLC)=POS_$SYNCVMLC;
       ]
       J=K;
     ]WHILE(J<=TOT_LEAF_$SYNCVMLC);
  ]
  CLOSE(UNIT=mlc_unit);

  OUTPUT;('******MLC SEQUENCE FILE READ******');
  IF (MODE_$SYNCVMLC=1) [
      OUTPUT; ('**dynamic delivery**');
  ]
  ELSEIF (MODE_$SYNCVMLC=2) [
      OUTPUT; ('**step and shoot delivery**');
  ]
]
ELSE[
OUTPUT; (' Input for MLC A and B leaf tips '/);
IF(ORIENT_$SYNCVMLC=1)[
    IF(ENDTYPE_$SYNCVMLC=1)[
      OUTPUT;(' Input min. X, max. X of top of opening in leaves,');
    ]
    ELSE[
      OUTPUT;(' Input min. X, max. X of rounded leaf ends,');
    ]
]
ELSE[
    IF(ENDTYPE_$SYNCVMLC=1)[
      OUTPUT;(' Input min. Y, max. Y of top of opening in leaves,');
    ]
    ELSE[
      OUTPUT;(' Input min. Y, max. Y of rounded leaf ends,');
    ]
]
OUTPUT;(' # of adjacent leaves with these coordinates:');
I=1;
LOOP[
  OUTPUT I;('   For leaf',I4,' :',$)
  MINPUT ($SYNCVMLC) NEG_$SYNCVMLC, POS_$SYNCVMLC, NUM_$SYNCVMLC; (2F15.0,I5);
  IF(NUM_$SYNCVMLC<=0) NUM_$SYNCVMLC=1;
  OUTPUT NEG_$SYNCVMLC, POS_$SYNCVMLC, NUM_$SYNCVMLC; (2F12.5,I5);
  IF(NEG_$SYNCVMLC > POS_$SYNCVMLC)[
     NEG_$SYNCVMLC = POS_$SYNCVMLC;
     OUTPUT ICM_$SYNCVMLC,I,I+NUM_$SYNCVMLC-1,NEG_$SYNCVMLC;
      (//' ***ERROR IN CM ',I4,' ($SYNCVMLC)'/
   ' Min. and max. opening coordinates in leaves ',I4,' - ',I4,' overlap'/
   ' Both coordinates set to ',F15.5,' cm for now'//);
     IERR_GEOM(ICM_$SYNCVMLC)=IERR_GEOM(ICM_$SYNCVMLC)+1;
                               ]

  IF(ABS(NEG_$SYNCVMLC)>RMAX_CM(ICM_$SYNCVMLC)|
     ABS(POS_$SYNCVMLC)>RMAX_CM(ICM_$SYNCVMLC)) [

     OUTPUT ICM_$SYNCVMLC,I,I+NUM_$SYNCVMLC-1;
      (//' ***ERROR IN CM ',I4,' ($SYNCVMLC)'/
      ' Tip of leaves ',I4,' - ',I4,' are outside CM '//);

     IERR_GEOM(ICM_$SYNCVMLC)=IERR_GEOM(ICM_$SYNCVMLC)+1;
                                            ];
  DO J=I,I+NUM_$SYNCVMLC-1["define opening for all leaves in group"
    IF(J>TOT_LEAF_$SYNCVMLC) EXIT;
    LEAFB_$SYNCVMLC(J)=NEG_$SYNCVMLC;
    LEAFA_$SYNCVMLC(J)=POS_$SYNCVMLC;
  ]
  I=J;
]WHILE(I<=TOT_LEAF_$SYNCVMLC);"End of Coordinate inputs for || direction "

IF(ENDTYPE_$SYNCVMLC=1)[
DO I=1, TOT_LEAF_$SYNCVMLC [

SURPARA2_B_$SYNCVMLC(I)=
    LEAFB_$SYNCVMLC(I)/(ZMIN_$SYNCVMLC-ZFOCUS_$SYNCVMLC(2) );

SURPARA2_A_$SYNCVMLC(I)=
    LEAFA_$SYNCVMLC(I)/(ZMIN_$SYNCVMLC-ZFOCUS_$SYNCVMLC(2) );

                         ];
                    ];
]


"X. ESTABLISH TOP OF FIRST CM
"==============================

ZFRONT_$SYNCVMLC = Z_min_CM(ICM_$SYNCVMLC);

"
"XI. ESTABLISH START OF NEXT CM
"================================

Z_min_CM(ICM_$SYNCVMLC+1) = ZMAX_$SYNCVMLC;

"
"XII. GET ECUT, PCUT, DOSE SCORING ZONE AND MATERIAL IN EACH REGION
"==================================================================

IRA = IRSTART_$SYNCVMLC-1;
DO IR_$SYNCVMLC = 1,N_$SYNCVMLC ["loop through regions to get information"
   IRA = IRA+1;
   IF(IR_$SYNCVMLC=1) [
       OUTPUT IR_$SYNCVMLC;
        (/' Region',I4,' (MLC opening):'/
          ' ECUT, PCUT (MeV), DOSE ZONE (0=NO DOSE SCORED), IREGION_TO_BIT'/
          ' :',$);
   ]
   ELSEIF(IR_$SYNCVMLC=2) [
       OUTPUT IR_$SYNCVMLC;
        (/' Region',I4,' (MLC leaves):'/
' ECUT, PCUT (MeV), DOSE ZONE (0=NO DOSE SCORED), IREGION_TO_BIT, IGNOREGAPS'/
         ' :',$);
   ]
   ELSE[
       OUTPUT IR_$SYNCVMLC;
        (/' Region',I4,' (driving screw holes):'/
         ' ECUT, PCUT (MeV), DOSE ZONE (0=NO DOSE SCORED), IREGION_TO_BIT'/
         ' :',$);
   ]
   IF(IR_$SYNCVMLC~=2)[
      MINPUT ($SYNCVMLC) ECUT(IRA),PCUT(IRA),DOSE_ZONE(IRA),IREGION_TO_BIT(IRA);
      (2F15.0,2I5);
      OUTPUT ECUT(IRA),PCUT(IRA),DOSE_ZONE(IRA),IREGION_TO_BIT(IRA);
             (2F15.5,2I5);
   ]
   ELSE[
      MINPUT ($SYNCVMLC) ECUT(IRA),PCUT(IRA),DOSE_ZONE(IRA),
                        IREGION_TO_BIT(IRA),IGNOREGAPS_$SYNCVMLC; (2F15.0,3I5);
      OUTPUT ECUT(IRA),PCUT(IRA),DOSE_ZONE(IRA),IREGION_TO_BIT(IRA),
             IGNOREGAPS_$SYNCVMLC; (2F15.5,3I5);
   ]
   IF(ECUT(IRA) < ECUTIN) [ECUT(IRA)=ECUTIN;];
   IF(PCUT(IRA) < PCUTIN) [PCUT(IRA)=PCUTIN;];
   OUTPUT IR_$SYNCVMLC; (' material of region ',I3,' ',$);
     $MED_INPUT($SYNCVMLC); " inputs character array MED_IN from unit 5, loops"
  "through array MEDIA(24,I) to check if medium was previously input."
  "If so, sets MED_INDEX to index of previous medium.  If not,"
  "increments NMED and sets MED_INDEX to NMED."
   MED(IRA) = MED_INDEX; " medium of the planar slab"
 ] "end of loop over IR_$SYNCVMLC"
;

IF(IGNOREGAPS_$SYNCVMLC=1 & IREJCT_GLOBAL>0)[
   IF(ORIENT_$SYNCVMLC=1)["leaves parallel to X"
       OUTPUT;(/' *******Range rejection in $SYNCVMLC will ignore all'/
               ' air gaps if the particle is in the leaves and has'/
               ' X < min. X of leaf openings (not including leaf ends)'/
               ' or X > max. X of leaf openings (not including ends)'/);
   ]
   ELSE["leaves parallel to Y"
       OUTPUT;(/' *******Range rejection in $SYNCVMLC will ignore all'/
               ' air gaps if the particle is in the leaves and has'/
               ' Y < min. Y of leaf openings (not including leaf ends)'/
               ' or Y > max. Y of leaf openings (not including ends)'/);
   ]
   IF(MODE_$SYNCVMLC=0)[
   DO I=1,TOT_LEAF_$SYNCVMLC["loop through leaves to find index of those with"
                           "max. +ve opening and min. -ve opening"
       IF(I=1)[
           MIN_INDEX=I;
           MAX_INDEX=I;
       ]
       ELSE[
           IF(LEAFB_$SYNCVMLC(I)<LEAFB_$SYNCVMLC(I-1))MIN_INDEX=I;
           IF(LEAFA_$SYNCVMLC(I)>LEAFA_$SYNCVMLC(I-1))MAX_INDEX=I;
       ]
   ]
   IF(ENDTYPE_$SYNCVMLC=1)["straight, focused ends"
       MIN_PLANE_$SYNCVMLC=MIN(LEAFB_$SYNCVMLC(MIN_INDEX),
                        SURPARA2_B_$SYNCVMLC(MIN_INDEX)*
                       (ZMIN_$SYNCVMLC+ZTHICK_$SYNCVMLC-ZFOCUS_$SYNCVMLC(2)));
       MAX_PLANE_$SYNCVMLC=MAX(LEAFB_$SYNCVMLC(MAX_INDEX),
                         SURPARA2_B_$SYNCVMLC(MAX_INDEX)*
                         (ZMIN_$SYNCVMLC+ZTHICK_$SYNCVMLC-ZFOCUS_$SYNCVMLC(2)));
   ]
   ELSE["rounded leaf ends"
       MIN_PLANE_$SYNCVMLC=LEAFB_$SYNCVMLC(MIN_INDEX)-LEAFRADIUS_$SYNCVMLC+
                         SQRT(LEAFRADIUS_$SYNCVMLC**2-(ZTHICK_$SYNCVMLC/2.)**2);
       MAX_PLANE_$SYNCVMLC=LEAFA_$SYNCVMLC(MAX_INDEX)+LEAFRADIUS_$SYNCVMLC-
                         SQRT(LEAFRADIUS_$SYNCVMLC**2-(ZTHICK_$SYNCVMLC/2.)**2);
   ]
   ]
]
ELSE["set to default"
   IGNOREGAPS_$SYNCVMLC=0;
]
"
"XIII. SET UP AIR GAP TO PREVIOUS CM IF PRESENT
"  =========================================
"
"  The air gap has the highest region number in the CM, even though its
"  the top of the component module.  This is to allow the assignment of
"  region numbers on input of the parameters of each local region
"  (mainly to assign the medium number of the region).
"  The air gap is then assigned after all of the
"  CM parameters have been input.
"
"note that if this is the first CM (ICM_$mlc=1) then the gap thickness
"Z_gap_THICK(ICM_$syncvmlc) = 0, which is used as a flag for no air gap

Z_gap_THICK(ICM_$SYNCVMLC) = ZMIN_$SYNCVMLC - Z_min_CM(ICM_$SYNCVMLC);
IF (Z_gap_THICK(ICM_$SYNCVMLC) < = 0.0) [
   Z_gap_THICK(ICM_$SYNCVMLC) = 0.;
   N_GAP_$SYNCVMLC = 0; "no air gap for this CM"
  ]
ELSE [
   N_GAP_$SYNCVMLC = 1; "this CM has an air gap"
   IRA = IRSTART_$SYNCVMLC+N_$SYNCVMLC; "absolute region number of air gap"
   MED(IRA) = AIR_INDEX; "medium is air"
 ];
"
"XIV. SET UP REGION NUMBERS
"===========================
"
"  This CM has N_$SYNCVMLC+N_GAP_$mlc regions
"
;
"Index last region
IREND_$SYNCVMLC = (IRSTART_$SYNCVMLC -1) + N_$SYNCVMLC+N_GAP_$SYNCVMLC;
NREG = NREG+N_$SYNCVMLC+N_GAP_$SYNCVMLC;
                            "Total no of regions in full geometry up
                            "to and including this CM
IF (NREG <= $MXREG) [
   IR_start_CM(ICM_$SYNCVMLC+1) = IREND_$SYNCVMLC+1;
  ]    "have not exceeded maximum region number
       "Index of first region in next CM:"
ELSE [
   OUTPUT ICM_$SYNCVMLC,NREG,$MXREG;
   (//' ***ERROR IN CM ',I4,' ($SYNCVMLC):'/
   T2,I4,' regions requested, only ',I4,' available'//);
   IERR_GEOM(ICM_$SYNCVMLC)=IERR_GEOM(ICM_$SYNCVMLC)+1;
 ];

"
"XV.  ESTABLISH CM BOUNDARY
"============================
"
RMAX_CM_FLAG(ICM_$SYNCVMLC) = 2; "put a square boundary about CM
"
"XVI. ESTABLISH DOSE SCORING ZONES AND BIT SETTING FOR EACH REGION
"===================================================================
"
IRA = IRSTART_$SYNCVMLC-1; "absolute region number"
DO IR_$SYNCVMLC=1,N_$SYNCVMLC ["loop over local region number"
   IRA = IRA+1;
   "dose-scoring zones"
   NDOSE_ZONE = MAX(DOSE_ZONE(IRA),NDOSE_ZONE); "Number of dose zones"
   MAX_BIT = MAX(IREGION_TO_BIT(IRA),MAX_BIT); " current maximum"
   "charged particle range rejection parameters"
   ESAVE(IRA)=ESAVE_GLOBAL; "Particles with total energies below ESAVE are"
                            "considered for range rejection"
   ECUTRR(IRA)=ECUT(IRA); "Minimum energy on exit from region GXD"
   E_min_out(ICM_$SYNCVMLC)=ECUT(IRA); "Minimum energy on exit from CM"
  ] "end of loop over IR_$SYNCVMLC

"XVII. ESTABLISH SUB-REGION IR VALUES
"===================================
DO L=1,TOT_LEAF_$SYNCVMLC [
      DO I=1,5 [ "NX"
           DO J =1,6 [ "NY"
                DO K = 1,9 [ "NZ"
                  SUBINDEX_$SYNCVMLC(L,I,J,K)=1; "set all regions to air"
                  ];
            ];
         ];
      ];
;
"Now define regions containing leaf medium"
DO L=1,TOT_LEAF_$SYNCVMLC[
  DO I =2,5 [
   DO J = 3,4 [
      DO K = 3,7 [
        SUBINDEX_$SYNCVMLC(L,I,J,K)=2; "set central section to leaf med"
       ];
     ];
   IF(LEAFTYPE_$SYNCVMLC(L)=2) ["This is a target leaf"
      SUBINDEX_$SYNCVMLC(L,I,1,1)=2;
      SUBINDEX_$SYNCVMLC(L,I,1,2)=2;
      SUBINDEX_$SYNCVMLC(L,I,1,3)=2;
      SUBINDEX_$SYNCVMLC(L,I,1,4)=2;
      SUBINDEX_$SYNCVMLC(L,I,1,5)=2;
      SUBINDEX_$SYNCVMLC(L,I,2,1)=2;
      SUBINDEX_$SYNCVMLC(L,I,2,2)=2;
      SUBINDEX_$SYNCVMLC(L,I,2,3)=2;
      SUBINDEX_$SYNCVMLC(L,I,2,4)=2;
      SUBINDEX_$SYNCVMLC(L,I,2,5)=2;
      SUBINDEX_$SYNCVMLC(L,I,2,6)=2;
      SUBINDEX_$SYNCVMLC(L,I,2,7)=2;
      SUBINDEX_$SYNCVMLC(L,I,3,1)=2;
      SUBINDEX_$SYNCVMLC(L,I,4,1)=2;
      SUBINDEX_$SYNCVMLC(L,I,4,8)=2;
      SUBINDEX_$SYNCVMLC(L,I,5,1)=2;
      SUBINDEX_$SYNCVMLC(L,I,5,3)=2;
      SUBINDEX_$SYNCVMLC(L,I,5,4)=2;
      SUBINDEX_$SYNCVMLC(L,I,5,5)=2;
      SUBINDEX_$SYNCVMLC(L,I,5,6)=2;
      SUBINDEX_$SYNCVMLC(L,I,6,3)=2;
      SUBINDEX_$SYNCVMLC(L,I,6,4)=2;
      SUBINDEX_$SYNCVMLC(L,I,6,5)=2;
      SUBINDEX_$SYNCVMLC(L,I,6,6)=2;
    ]
   ELSEIF(LEAFTYPE_$SYNCVMLC(L)=1) ["full leaf"
      SUBINDEX_$SYNCVMLC(L,I,1,2)=2;
      SUBINDEX_$SYNCVMLC(L,I,1,3)=2;
      SUBINDEX_$SYNCVMLC(L,I,2,2)=2;
      SUBINDEX_$SYNCVMLC(L,I,2,3)=2;
      SUBINDEX_$SYNCVMLC(L,I,2,4)=2;
      SUBINDEX_$SYNCVMLC(L,I,2,5)=2;
      SUBINDEX_$SYNCVMLC(L,I,2,6)=2;
      SUBINDEX_$SYNCVMLC(L,I,2,7)=2;
      SUBINDEX_$SYNCVMLC(L,I,4,9)=2;
      SUBINDEX_$SYNCVMLC(L,I,5,3)=2;
      SUBINDEX_$SYNCVMLC(L,I,5,4)=2;
      SUBINDEX_$SYNCVMLC(L,I,5,5)=2;
      SUBINDEX_$SYNCVMLC(L,I,5,6)=2;
      SUBINDEX_$SYNCVMLC(L,I,5,7)=2;
      SUBINDEX_$SYNCVMLC(L,I,5,8)=2;
      SUBINDEX_$SYNCVMLC(L,I,5,9)=2;
      SUBINDEX_$SYNCVMLC(L,I,6,5)=2;
      SUBINDEX_$SYNCVMLC(L,I,6,6)=2;
      SUBINDEX_$SYNCVMLC(L,I,6,7)=2;
      SUBINDEX_$SYNCVMLC(L,I,6,8)=2;
      SUBINDEX_$SYNCVMLC(L,I,6,9)=2;
    ]
   ELSE[ "This is an isocenter leaf"
      SUBINDEX_$SYNCVMLC(L,I,1,4)=2;
      SUBINDEX_$SYNCVMLC(L,I,1,5)=2;
      SUBINDEX_$SYNCVMLC(L,I,1,6)=2;
      SUBINDEX_$SYNCVMLC(L,I,1,7)=2;
      SUBINDEX_$SYNCVMLC(L,I,2,4)=2;
      SUBINDEX_$SYNCVMLC(L,I,2,5)=2;
      SUBINDEX_$SYNCVMLC(L,I,2,6)=2;
      SUBINDEX_$SYNCVMLC(L,I,2,7)=2;
      SUBINDEX_$SYNCVMLC(L,I,2,9)=2;
      SUBINDEX_$SYNCVMLC(L,I,3,2)=2;
      SUBINDEX_$SYNCVMLC(L,I,3,9)=2;
      SUBINDEX_$SYNCVMLC(L,I,4,9)=2;
      SUBINDEX_$SYNCVMLC(L,I,5,3)=2;
      SUBINDEX_$SYNCVMLC(L,I,5,4)=2;
      SUBINDEX_$SYNCVMLC(L,I,5,5)=2;
      SUBINDEX_$SYNCVMLC(L,I,5,6)=2;
      SUBINDEX_$SYNCVMLC(L,I,5,7)=2;
      SUBINDEX_$SYNCVMLC(L,I,5,8)=2;
      SUBINDEX_$SYNCVMLC(L,I,5,9)=2;
      SUBINDEX_$SYNCVMLC(L,I,6,5)=2;
      SUBINDEX_$SYNCVMLC(L,I,6,6)=2;
      SUBINDEX_$SYNCVMLC(L,I,6,7)=2;
      SUBINDEX_$SYNCVMLC(L,I,6,8)=2;
      SUBINDEX_$SYNCVMLC(L,I,6,9)=2;
    ];
  ];
"Now set driving screw hole to air"
   DO J=1,6 [
    IF(LEAFTYPE_$SYNCVMLC(L)=2) ["target leaf"
      SUBINDEX_$SYNCVMLC(L,2,J,4)=3;
      SUBINDEX_$SYNCVMLC(L,5,J,4)=3;
     ]
    ELSE ["full or isocenter leaf"
      SUBINDEX_$SYNCVMLC(L,2,J,6)=3;
      SUBINDEX_$SYNCVMLC(L,5,J,6)=3;
     ]
   ];
   IF(LEAFTYPE_$SYNCVMLC(L)=1) ["full leaf"
     SUBINDEX_$SYNCVMLC(L,2,1,6)=1;
     SUBINDEX_$SYNCVMLC(L,5,1,6)=1;
   ];
 ];
  "   =======================
"
RETURN;

"XVIII. ERROR MESSAGES
"==================
"
:EOF_$SYNCVMLC:
;OUTPUT ICM;
  (//' *** ERROR *** unexpected end of file reading input for CM',I3);
STOP;

:ERROR_$SYNCVMLC:
;OUTPUT ICM;(//' *** ERROR *** format error on input for CM',I3);
STOP;

:no-mlc-data-file:
;OUTPUT;(//' *** ERROR: MLC leaf data file could not be opened');
STOP;

END;  "End of INPUT_$SYNCVMLC"

%E "Start of subroutine ISUMRY_$SYNCVMLC (Rev 1.4)"
"***********************************************************************
"
"                          Subroutine ISUMRY_$SYNCVMLC
"                          ***********************
"
" Summarize input, write graphics file for EGS_Windows, and set parameters
" that require medium information obtained from HATCH call.
"
"***********************************************************************

;SUBROUTINE ISUMRY_$SYNCVMLC;

;IMPLICIT NONE;
;COMIN/ BOUNDS,CMs,CM_$SYNCVMLC,GEOM,IO_INFO,MEDIA,MISC,SCORE,UPHIOT,USER/;
"T>
"T>**********************************
"T>TYPE DECLARATIONS FOR ISUMRY_$SYNCVMLC
"T>**********************************
"T>
INTEGER
   ICOLOUR, "T>colour of CM for EGS_Windows
"   ID,     already defined gf  T>index of dose scoring zone
   IRA,     "T>absolute region number
   I,J,     "T>DO loop index
   ISTART,IEND, "T>indices for outputting info"
   YY1(9),          "T> YY1 and YY2 index subregion along perpendicular..
   YY2(9),          "T> direction to MLC orientation
   YY3(9),          "T> specific for target leaf
   YY4(9),          "T>
   YY5(9),          "T> same for full leaf
   YY6(9),          "T>
   YY7(9),          "T> same for isocenter leaf
   YY8(9);         "T>

$REAL  VOL_$SYNCVMLC(3), "T> region volumes
       Zo,             "T> THe z-coordinate of the leaf center
       XoP,XoN,        "T> The center of the rounded leaves ends along leaf
       M1,M2,          "T> Variables related to partial(rounded) leaf volume
       THETASUB,       "T> Angle at leaf end subtended by Z boundaries of reg.
       TOTALVOL,       "T> Total volume of MLC CM
       ZSQUARE,ZCUBE,AREA,    "T> Variables related to partial leaf volumes
       TEMP,TEMP1,TEMP2,TEMP3,"T> Variables related to partial leaf volumes
       HOLD;            "T> to hold a TEMP value

"   Mass of dose zone
"   =================
"
"Need to calculate mass of dose zone here, after call to HATCH, where the
"region density is set if it was allowed to default in INPUT_$mlc.
"This volume calculation is approximate, but close enough
"

TOTALVOL = (ZMAX_$SYNCVMLC-ZMIN_$SYNCVMLC)*4*RMAX_CM(ICM_$SYNCVMLC)**2;
VOL_$SYNCVMLC(1) = 0.;
VOL_$SYNCVMLC(2) = 0.;
VOL_$SYNCVMLC(3) = 0.;

Zo = ZMIN_$SYNCVMLC + (ZTHICK_$SYNCVMLC/2);

DATA ( YY3(I), I=1,9) /1,1,1,1,1,2,2,4,7/; "target leaf"
DATA ( YY4(I), I=1,9) /6,3,7,7,7,7,5,5,7/;
DATA ( YY5(I), I=1,9) /1,1,1,2,2,2,2,5,4/; "full leaf"
DATA ( YY6(I), I=1,9) /1,3,6,6,7,7,7,7,7/;
DATA ( YY7(I), I=1,9) /1,3,3,1,1,1,1,5,2/; "isocenter leaf"
DATA ( YY8(I), I=1,9) /1,4,6,6,7,7,7,7,7/;

DO J = 1, TOT_LEAF_$SYNCVMLC [
  IF(LEAFTYPE_$SYNCVMLC(J)=2) [
    DO I=1,9[
      YY1(I)=YY3(I);
      YY2(I)=YY4(I);
    ]
   ]
  ELSEIF(LEAFTYPE_$SYNCVMLC(J)=1) [
    DO I=1,9[
      YY1(I)=YY5(I);
      YY2(I)=YY6(I);
    ]
   ]
  ELSE [
    DO I=1,9[
      YY1(I)=YY7(I);
      YY2(I)=YY8(I);
    ]
   ]
  DO I = 2,10 [ "9 subregions in z-direction"
      ZSQUARE=(ZREG_$SYNCVMLC(J,I)**2)-(ZREG_$SYNCVMLC(J,I-1)**2) -
              2*ZREG_$SYNCVMLC(J,I)*ZFOCUS_$SYNCVMLC(1) +
              2*ZREG_$SYNCVMLC(J,I-1)*ZFOCUS_$SYNCVMLC(1);
      AREA=SURPARA1_$SYNCVMLC(J,YY2(I-1))/2*ZSQUARE -
               SURPARA1_$SYNCVMLC(J,YY1(I-1))/2*ZSQUARE;
      IF(ENDTYPE_$SYNCVMLC=0) [ "Rounded leaf end"
        XoN=LEAFB_$SYNCVMLC(J)-LEAFRADIUS_$SYNCVMLC;
        XoP=LEAFA_$SYNCVMLC(J)+LEAFRADIUS_$SYNCVMLC;
        TEMP1=SQRT(LEAFRADIUS_$SYNCVMLC**2-(ZREG_$SYNCVMLC(J,I-1)-Zo)**2);
        TEMP2=SQRT(LEAFRADIUS_$SYNCVMLC**2-(ZREG_$SYNCVMLC(J,I)-Zo)**2);
        M1=ASIN(TEMP1/LEAFRADIUS_$SYNCVMLC);
        M2=ASIN(TEMP2/LEAFRADIUS_$SYNCVMLC);
        THETASUB=ABS(M1-M2);
        TEMP=AREA*(2*RMAX_CM(ICM_$SYNCVMLC)-(XoP-MIN(TEMP1,TEMP2)-
                      XoN-MIN(TEMP1,TEMP2)));
        TEMP=TEMP+((ZREG_$SYNCVMLC(J,I)-ZREG_$SYNCVMLC(J,I-1))*ABS(TEMP1-TEMP2)
             +LEAFRADIUS_$SYNCVMLC**2*(THETASUB-SIN(THETASUB)))*
             ((SURPARA1_$SYNCVMLC(J,YY2(I-1))-SURPARA1_$SYNCVMLC(J,YY1(I-1)))*
             (ZREG_$SYNCVMLC(J,I)-ZFOCUS_$SYNCVMLC(1)) +
             (SURPARA1_$SYNCVMLC(J,YY2(I-1))-SURPARA1_$SYNCVMLC(J,YY1(I-1)))*
             (ZREG_$SYNCVMLC(J,I-1)-ZFOCUS_$SYNCVMLC(1)))/2;
        VOL_$SYNCVMLC(2)=VOL_$SYNCVMLC(2)+TEMP;
       ]
      ELSE[ "Focused divergent leaf end "
        TEMP1=SURPARA2_A_$SYNCVMLC(J)*
              (ZREG_$SYNCVMLC(J,I-1)-ZFOCUS_$SYNCVMLC(2));
        TEMP2=SURPARA2_A_$SYNCVMLC(J)*(ZREG_$SYNCVMLC(J,I)-ZFOCUS_$SYNCVMLC(2));
        "do positive part of leaf first"
        TEMP=AREA*(RMAX_CM(ICM_$SYNCVMLC)-MAX(TEMP1,TEMP2));
        TEMP=TEMP+(ABS(TEMP1-TEMP2)*(ZREG_$SYNCVMLC(J,I)-
             ZREG_$SYNCVMLC(J,I-1)))/2*((SURPARA1_$SYNCVMLC(J,YY2(I-1))-
             SURPARA1_$SYNCVMLC(J,YY1(I-1)))*(ZREG_$SYNCVMLC(J,I)-
             ZFOCUS_$SYNCVMLC(1))+(SURPARA1_$SYNCVMLC(J,YY2(I-1))-
             SURPARA1_$SYNCVMLC(J,YY1(I-1)))*(ZREG_$SYNCVMLC(J,I-1)-
             ZFOCUS_$SYNCVMLC(1)))/2;
        "now do negative part of leaf"
        TEMP1=SURPARA2_B_$SYNCVMLC(J)*
              (ZREG_$SYNCVMLC(J,I-1)-ZFOCUS_$SYNCVMLC(2));
        TEMP2=SURPARA2_B_$SYNCVMLC(J)*(ZREG_$SYNCVMLC(J,I)-ZFOCUS_$SYNCVMLC(2));
        TEMP=TEMP+AREA*(MIN(TEMP1,TEMP2)+RMAX_CM(ICM_$SYNCVMLC));
        TEMP=TEMP+(ABS(TEMP1-TEMP2)*(ZREG_$SYNCVMLC(J,I)-
             ZREG_$SYNCVMLC(J,I-1)))/2*((SURPARA1_$SYNCVMLC(J,YY2(I-1))-
             SURPARA1_$SYNCVMLC(J,YY1(I-1)))*(ZREG_$SYNCVMLC(J,I)-
             ZFOCUS_$SYNCVMLC(1))+(SURPARA1_$SYNCVMLC(J,YY2(I-1))-
             SURPARA1_$SYNCVMLC(J,YY1(I-1)))*(ZREG_$SYNCVMLC(J,I-1)-
             ZFOCUS_$SYNCVMLC(1)))/2;
        VOL_$SYNCVMLC(2)=VOL_$SYNCVMLC(2)+TEMP;
       ];

     "IF(I=5 & J=1) HOLD=TEMP;  <-- not using this for now"
    ];  "End of I loop"
    "now subtract volume of driving screw hole"
   IF(LEAFTYPE_$SYNCVMLC(J)=2) ["target leaf"
     ZSQUARE=(ZREG_$SYNCVMLC(J,5)**2)-(ZREG_$SYNCVMLC(J,4)**2) -
              2*ZREG_$SYNCVMLC(J,5)*ZFOCUS_$SYNCVMLC(1) +
              2*ZREG_$SYNCVMLC(J,4)*ZFOCUS_$SYNCVMLC(1);
     AREA=SURPARA1_$SYNCVMLC(J,7)/2*ZSQUARE -
               SURPARA1_$SYNCVMLC(J,1)/2*ZSQUARE;
     "positive portion of leaf"
     TEMP=AREA*(RMAX_CM(ICM_$SYNCVMLC)-LEAFA_$SYNCVMLC(J)-HOLEPOS_$SYNCVMLC(J));
     VOL_$SYNCVMLC(2)=VOL_$SYNCVMLC(2)-TEMP;
     VOL_$SYNCVMLC(3)=VOL_$SYNCVMLC(3)+TEMP;
     "negative portion of leaf"
     TEMP=AREA*(LEAFB_$SYNCVMLC(J)-HOLEPOS_$SYNCVMLC(J)+RMAX_CM(ICM_$SYNCVMLC));
     VOL_$SYNCVMLC(2)=VOL_$SYNCVMLC(2)-TEMP;
     VOL_$SYNCVMLC(3)=VOL_$SYNCVMLC(3)+TEMP;
    ]
  IF(LEAFTYPE_$SYNCVMLC(J)=1) ["full leaf"
     ZSQUARE=(ZREG_$SYNCVMLC(J,7)**2)-(ZREG_$SYNCVMLC(J,6)**2) -
              2*ZREG_$SYNCVMLC(J,7)*ZFOCUS_$SYNCVMLC(1) +
              2*ZREG_$SYNCVMLC(J,6)*ZFOCUS_$SYNCVMLC(1);
     AREA=SURPARA1_$SYNCVMLC(J,7)/2*ZSQUARE -
               SURPARA1_$SYNCVMLC(J,2)/2*ZSQUARE;
     "positive portion of leaf"
     TEMP=AREA*(RMAX_CM(ICM_$SYNCVMLC)-LEAFA_$SYNCVMLC(J)-HOLEPOS_$SYNCVMLC(J));
     VOL_$SYNCVMLC(2)=VOL_$SYNCVMLC(2)-TEMP;
     VOL_$SYNCVMLC(3)=VOL_$SYNCVMLC(3)+TEMP;
     "negative portion of leaf"
     TEMP=AREA*(LEAFB_$SYNCVMLC(J)-HOLEPOS_$SYNCVMLC(J)+RMAX_CM(ICM_$SYNCVMLC));
     VOL_$SYNCVMLC(2)=VOL_$SYNCVMLC(2)-TEMP;
     VOL_$SYNCVMLC(3)=VOL_$SYNCVMLC(3)+TEMP;
    ]
  ELSE ["isocenter leaf"
     ZSQUARE=(ZREG_$SYNCVMLC(J,7)**2)-(ZREG_$SYNCVMLC(J,6)**2) -
              2*ZREG_$SYNCVMLC(J,7)*ZFOCUS_$SYNCVMLC(1) +
              2*ZREG_$SYNCVMLC(J,6)*ZFOCUS_$SYNCVMLC(1);
     AREA=SURPARA1_$SYNCVMLC(J,7)/2*ZSQUARE -
               SURPARA1_$SYNCVMLC(J,1)/2*ZSQUARE;
     "positive portion of leaf"
     TEMP=AREA*(RMAX_CM(ICM_$SYNCVMLC)-LEAFA_$SYNCVMLC(J)-HOLEPOS_$SYNCVMLC(J));
     VOL_$SYNCVMLC(2)=VOL_$SYNCVMLC(2)-TEMP;
     VOL_$SYNCVMLC(3)=VOL_$SYNCVMLC(3)+TEMP;
     "negative portion of leaf"
     TEMP=AREA*(LEAFB_$SYNCVMLC(J)-HOLEPOS_$SYNCVMLC(J)+RMAX_CM(ICM_$SYNCVMLC));
     VOL_$SYNCVMLC(2)=VOL_$SYNCVMLC(2)-TEMP;
     VOL_$SYNCVMLC(3)=VOL_$SYNCVMLC(3)+TEMP;
    ]

 ];       "End of J Loop"

VOL_$SYNCVMLC(1) = TOTALVOL - VOL_$SYNCVMLC(2) - VOL_$SYNCVMLC(3);

"i.e. Air Volume = Total Volume - Volume of Leaves

IRA = IRSTART_$SYNCVMLC-1; "absolute region number
DO IR_$SYNCVMLC=1,N_$SYNCVMLC [ "loop over local region number"
   IRA = IRA+1;
   IF(DOSE_ZONE(IRA).NE.0) ["this is a dose zone"
      ID=DOSE_ZONE(IRA);
      AMASS(ID)=AMASS(ID)+VOL_$SYNCVMLC(IR_$SYNCVMLC)*RHOR(IRA);
    ];
 ]; "end of loop over IR_$SYNCVMLC"

"   Summarize geometrical information for this CM in listing file
"   ==============================================================
"
WRITE(IOUTLIST,110) ICM_$SYNCVMLC,TITLE_$SYNCVMLC;
WRITE(IOUTLIST,120) Z_min_CM(ICM_$SYNCVMLC), RMAX_CM(ICM_$SYNCVMLC);
IF(N_GAP_$SYNCVMLC~=0) [
   WRITE(IOUTLIST,124) Z_min_CM(ICM_$SYNCVMLC),
                       ZMIN_$SYNCVMLC-Z_min_CM(ICM_$SYNCVMLC);
  ];
WRITE(IOUTLIST,122) ZTHICK_$SYNCVMLC;
IF(ORIENT_$SYNCVMLC~=0) [
   WRITE(IOUTLIST,123) 'X';
]
ELSE[
   WRITE(IOUTLIST,123) 'Y';
]
IF(ENDTYPE_$SYNCVMLC=1) [
   WRITE(IOUTLIST,125) ZFOCUS_$SYNCVMLC(2);
]
ELSE[
   WRITE(IOUTLIST,126) LEAFRADIUS_$SYNCVMLC;
]
WRITE(IOUTLIST,127) ZFOCUS_$SYNCVMLC(1),LEAFGAP_$SYNCVMLC,
                     VOL_$SYNCVMLC(1),VOL_$SYNCVMLC(2),
                     VOL_$SYNCVMLC(3),TOTALVOL;
WRITE(IOUTLIST,128) (LEAFWIDTH_$SYNCVMLC(I),I=1,3),
                    (WTONGUE_$SYNCVMLC(I),I=1,3),
                    (WGROOVE_$SYNCVMLC(I),I=1,3),
                    (WTIP_$SYNCVMLC(I),I=1,3),
                    (WRAILTOP_$SYNCVMLC(I),I=1,3),
                    (WRAILBOT_$SYNCVMLC(I),I=1,3),
                    (ZTIP_$SYNCVMLC(I),I=1,3),
                    (ZLEAF_$SYNCVMLC(I),I=1,3),
                    (ZTONGUE_$SYNCVMLC(I),I=1,3),
                    (ZGROOVE_$SYNCVMLC(I),I=1,3),
                    (ZHOLETOP_$SYNCVMLC(I),I=1,3),
                    (ZHOLEBOT_$SYNCVMLC(I),I=1,3),
                     HOLEPOS_FULL_$SYNCVMLC,HOLEPOS_TAR_$SYNCVMLC,
                     HOLEPOS_ISO_$SYNCVMLC,
                    (ZRAILTOP_$SYNCVMLC(I),I=1,3),
                    (ZRAILBOT_$SYNCVMLC(I),I=1,3);
IF(ORIENT_$SYNCVMLC~=0) [
      WRITE(IOUTLIST,129)'Y','X';
 ]
ELSE [
      WRITE(IOUTLIST,129)'X','Y';
]
ISTART=1;
IEND=2;
LOOP[
  IF(IEND>TOT_LEAF_$SYNCVMLC |
     LEAFB_$SYNCVMLC(IEND)~=LEAFB_$SYNCVMLC(ISTART)|
     LEAFA_$SYNCVMLC(IEND)~=LEAFA_$SYNCVMLC(ISTART)|
     (LEAFTYPE_$SYNCVMLC(ISTART)=1 &
        LEAFTYPE_$SYNCVMLC(IEND)~=LEAFTYPE_$SYNCVMLC(ISTART))|
     (LEAFTYPE_$SYNCVMLC(ISTART)>1 & LEAFTYPE_$SYNCVMLC(IEND)=1))[
     IF(LEAFTYPE_$SYNCVMLC(ISTART)=1)[
        WRITE(IOUTLIST,130)ISTART,IEND-1,'1',
                       YREG_$SYNCVMLC(ISTART,1),YREG_$SYNCVMLC(IEND-1,7),
                       LEAFB_$SYNCVMLC(ISTART),LEAFA_$SYNCVMLC(ISTART);
     ]
     ELSE[
        WRITE(IOUTLIST,130)ISTART,IEND-1,'2/3',
                       YREG_$SYNCVMLC(ISTART,1),YREG_$SYNCVMLC(IEND-1,7),
                       LEAFB_$SYNCVMLC(ISTART),LEAFA_$SYNCVMLC(ISTART);
     ]
     ISTART=IEND;
  ]
  ELSE[
     IEND=IEND+1;
  ]
]WHILE(ISTART<=TOT_LEAF_$SYNCVMLC);
WRITE(IOUTLIST,131);
IRA=IRSTART_$SYNCVMLC-1;
DO IR_$SYNCVMLC=1,N_$SYNCVMLC [
   IRA = IRA+1;
   IF(IR_$SYNCVMLC=1) [
      IF (MED(IRA)=0) ["Medium is vacuum"
         WRITE(IOUTLIST,140) IR_$SYNCVMLC,'opening',ECUT(IRA),PCUT(IRA),
           ECUTRR(IRA),ESAVE(IRA),DOSE_ZONE(IRA),
           IREGION_TO_BIT(IRA),'V','a','c','u','u','m';
       ]
      ELSE ["Medium is not vacuum"
         WRITE(IOUTLIST,140) IR_$SYNCVMLC,'opening',ECUT(IRA),PCUT(IRA),
         ECUTRR(IRA),ESAVE(IRA),DOSE_ZONE(IRA),
         IREGION_TO_BIT(IRA),(MEDIA(J,MED(IRA)),J=1,9);
      ];
   ]
   ELSEIF(IR_$SYNCVMLC=2 | IR_$SYNCVMLC=3) [
      IF (MED(IRA)=0) ["Medium is vacuum"
         WRITE(IOUTLIST,140) IR_$SYNCVMLC,'leaves',ECUT(IRA),PCUT(IRA),
           ECUTRR(IRA),ESAVE(IRA),DOSE_ZONE(IRA),
           IREGION_TO_BIT(IRA),'V','a','c','u','u','m';
       ]
      ELSE ["Medium is not vacuum"
         WRITE(IOUTLIST,140) IR_$SYNCVMLC,'leaves',ECUT(IRA),PCUT(IRA),
         ECUTRR(IRA),ESAVE(IRA),DOSE_ZONE(IRA),
         IREGION_TO_BIT(IRA),(MEDIA(J,MED(IRA)),J=1,9)
       ]
   ];
];

IF(N_GAP_$SYNCVMLC~=0) [
   IRA=IRSTART_$SYNCVMLC+N_$SYNCVMLC;
   WRITE(IOUTLIST,140) IR_$SYNCVMLC,'airgap',ECUT(IRA),PCUT(IRA),
         ECUTRR(IRA),ESAVE(IRA),DOSE_ZONE(IRA),IREGION_TO_BIT(IRA),
         (MEDIA(J,MED(IRA)),J=1,9);
   WRITE(IOUTLIST,141)'at top'
 ];

IF(IGNOREGAPS_$SYNCVMLC=1 & IREJCT_GLOBAL>0)[
  IF(ORIENT_$SYNCVMLC=1)["leaves parallel to X"
   WRITE(IOUTLIST,'(/'' *******Range rejection in $SYNCVMLC will ignore all''/
                   '' air gaps if the particle is in the leaves and has''/
                   '' X < min. X of leaf openings (not including leaf ends)''/
                   '' or X > max. X of leaf openings (not including ends)''/)');
  ]
  ELSE["leaves parallel to Y"
   WRITE(IOUTLIST,'(/'' *******Range rejection in $SYNCVMLC will ignore all''/
                   '' air gaps if the particle is in the leaves and has''/
                   '' Y < min. Y of leaf openings (not including leaf ends)''/
                   '' or Y > max. Y of leaf openings (not including ends)''/)');
  ]
]

110 FORMAT(/'  Component module',I3,' is a multi-leaf collimator',
           /' (3 regions Rev 1.4)',
           /'  -------------------------------------------------',
           /'------------------',
           //T5,'Title: ',68A1);
120 FORMAT(/T2,'$SYNCVMLC geometry parameters:',
           /T2,'------------------------',
           /T2,'Distance from front of CM from reference plane = ',T51,
           F15.5,' cm',
           /T2,'Radius of outer boundary of CM = ',T51,F15.5,' cm');
122 FORMAT(T2,'Thickness of collimator = ',T51,F15.5,' cm');
123 FORMAT(T2,'Leaves open parallel to the ',A1,' axis');
124 FORMAT(T2,'There is an airgap starting at Z = ',F8.3,' cm with',
              ' thickness ',F8.3,' cm');
125 FORMAT(T2,'Straight leaf ends with Z focus = ',T51,F15.5,' cm');
126 FORMAT(T2,'Cylindrical leaf ends with radius = ',T51,F15.5,' cm');
127 FORMAT(T2,'Z focus of leaf sides = ',T51,F15.5,' cm'/
T2,'Gap between adjacent leaves = ',T51,F15.5,' cm'/
T2,'Volume of region between leaves = ',T51,F15.5, 'cm**3'/
T2,'Volume of leaves = ',T51,F15.5, 'cm**3'/
T2,'Volume of driving screw holes = ',T51,F15.5, 'cm**3'/
T2,'Volume of Multileaf Collimator Component Module = ',T51,F15.5, 'cm**3');
128 FORMAT(/T2,'Dimensions for leaf types (all in cm):'/
T2,
'                                        FULL(1)     TARGET(2)   ISOCENTER(3)'/
T2,'                                         ----         ------     ---------'/
T2,'        Leaf width (excl. tongue):',3F13.5/
T2,'                  Width of tongue:',3F13.5/
T2,'                  Width of groove:',3F13.5/
T2,'                     Width of tip:',3F13.5/
T2,'      Width of upper support rail:',3F13.5/
T2,'      Width of lower support rail:',3F13.5/
T2,'           Z of top/bottom of tip:',3F13.5/
T2,'          Z of top/bottom of leaf:',3F13.5/
T2,'        Z of top/bottom of tongue:',3F13.5/
T2,'        Z of top/bottom of groove:',3F13.5/
T2,'   Z of top of driving screw hole:',3F13.5/
T2,'Z of bottom of driving screw hole:',3F13.5/
T2,'   Distance of hole from leaf tip:',3F13.5/
T2,'         Z of top of support rail:',3F13.5/
T2,'      Z of bottom of support rail:',3F13.5);
129 FORMAT(/
T2,' Leaves     type            ',A1,' range             ',A1,
' coordinates of opening'/
T2,'                           spanned (cm)              NEG   (cm)   POS');

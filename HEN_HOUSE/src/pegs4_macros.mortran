     NELEPS,ZEPST(20),IAPRFL,IAPRIMP;
     $INTEGER    ZEPST,NELEPS,IAPRFL,NEPST,IEPST,EPSTFLP,IAPRIMP;
     CHARACTER   EPSTTL*80;

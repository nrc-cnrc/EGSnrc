rewind(56);
READ(56,:FLT:) $LGN(RLC,AE,AP,UE,UP(IM));
READ(56,:INT:)
$LGN(MSGE,MGE,MSEKE,MEKE,MLEKE,MCMFP,MRANGE(IM));
READ(56,:FLT:)($LGN(DL(I,IM)/1,2,3,4,5,6/),I=1,6);
READ(56,:FLT:)DELCM(IM),($LGN(ALPHI,BPAR,
   DELPOS(I,IM)),I=1,2);

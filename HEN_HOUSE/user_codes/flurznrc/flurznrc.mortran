"OUTPUT DATEN,TIMEN;(/' END OF RUN',10X,$DATEN_FORMAT,1X,$TIMEN_FORMAT//);
OUTPUT; (/' END OF RUN',9X,' ',$); call egs_fdate(6); write(6,'(//)');

IF (ERROR_FLAG=1) [
   OUTPUT ;(//' **********  ERRORS AND WARNING MESSAGES  ********** '//);
]


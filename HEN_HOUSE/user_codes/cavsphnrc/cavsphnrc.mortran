            ]
        IHRSTL=IHRSTR+1;
        IF(IHRST+1.EQ.NHRST)[IHRSTR=NR;]
        ELSE[IHRSTR=IHRSTR+6;]
        ]   "END OF DO LOOP"
    ]   "END OF DOESN'T FIT"

"CALL PRNTER(13,6,IOUT,0);"

RETURN;

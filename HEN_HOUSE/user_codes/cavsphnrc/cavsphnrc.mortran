write(iout,'(/a,$)') '********* FINAL RANDOM NUMBER STATE:';
$SHOW-RNG-STATE(iout); write(iout,'(a)') '  *********';

$SET_ELAPSED_TOTAL_TIME(TIMEB);
$SET_ELAPSED_CPUTIME(CPUT2);
TIMCPU=(CPUT2-CPUT1)*$CONVERSION_TO_SECONDS+TMCPUO;
"**************************************"

REPLACE {$SELECT-ELECTRON-MFP;} WITH {;
    IF(ICSDA.EQ.0)[
        ;$RANDOMSET RNNE1;IF(RNNE1.EQ.0.0)[RNNE1=1.E-30;]
        "DEMFP=AMAX1(-ALOG(RNNE1),$EPSEMFP);"
        DEMFP=MAX(-LOG(RNNE1),$EPSEMFP);
        ]
    ELSE[
        DEMFP=VACDST;
        ]
;}


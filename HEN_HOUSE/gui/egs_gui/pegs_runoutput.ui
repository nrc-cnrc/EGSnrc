<class>PEGS_RunOutput</class>
 <widget class="QWidget" name="PEGS_RunOutput">
  <property name="geometry">
   <rect>
    <x>0</x>
    <y>0</y>
    <width>592</width>
    <height>663</height>
   </rect>
  </property>
  <property name="windowTitle">
   <string>PEGS Output</string>
  </property>
  <layout class="QVBoxLayout">
   <item>
    <widget class="QTextEdit" name="run_output"/>
   </item>
   <item>
    <layout class="QHBoxLayout">
     <item>
      <spacer name="spacer6">
       <property name="orientation">
        <enum>Qt::Horizontal</enum>
       </property>
       <property name="sizeType">
        <enum>QSizePolicy::Expanding</enum>
       </property>
       <property name="sizeHint" stdset="0">
        <size>
         <width>396</width>
         <height>21</height>
        </size>
       </property>
      </spacer>
     </item>
     <item>
      <widget class="QPushButton" name="pushButton8">
       <property name="text">
        <string>&amp;Close</string>
       </property>
      </widget>
     </item>
    </layout>
   </item>
  </layout>
 </widget>
 <layoutdefault spacing="6" margin="11"/>
 <customwidgets>
 </customwidgets>
 <resources/>
 <connections>
  <connection>
   <sender>pushButton8</sender>
   <signal>clicked()</signal>
   <receiver>PEGS_RunOutput</receiver>
   <slot>hideWindow()</slot>
   <hints>
    <hint type="sourcelabel">
     <x>20</x>
     <y>20</y>
    </hint>
    <hint type="destinationlabel">
     <x>20</x>
     <y>20</y>
    </hint>
   </hints>
  </connection>
 </connections>
